@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ff: <https://foerderfunke.org/default#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .

# general

ff:interestInBusinessFunding a ff:DataField ;
    schema:category ff:selbstaendigkeit-gruendung ;
    rdfs:label "Interest in Business Funding"@en, "Interesse an Unternehmensförderung"@de ;
    schema:question "Are you interested in funding opportunities for businesses?"@en, 
                    "Haben Sie Interesse an Fördermöglichkeiten für Unternehmen?"@de ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:interestInBusinessFunding ;
        sh:property [
            sh:path ff:interestInBusinessFunding ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;
    ] .


ff:housingProjectType a ff:DataField ;
    schema:category ff:wohnungsbau-df-kategorie ;
    rdfs:label "Art des Wohnprojekts"@de , "Type of Housing Project"@en ;
    schema:question "Welche Art von Wohnprojekt ist geplant?"@de ,
                    "What type of housing project is planned?"@en ;
    rdfs:comment "Erfasst wird die grundlegende Ausrichtung des geplanten Wohnprojekts, zum Beispiel Barriereabbau, erhöhter Wohnkomfort oder Einbruchschutz."@de ,
                 "This field captures the basic type of the planned housing project, for example barrier reduction, improved living comfort, or burglary protection."@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:housingProjectType ;
        sh:property [
            sh:path ff:housingProjectType ;
            sh:in (
                ff:housingProjectType-barrierReduction
                ff:housingProjectType-livingComfort
                ff:housingProjectType-burglaryProtection
                ff:housingProjectType-other
            ) ;
            sh:maxCount 1 ;
        ] ;
    ] .
ff:housingProjectType-barrierReduction a ff:AnswerOption ; rdfs:label "Barriereabbau"@de , "Barrier reduction"@en .
ff:housingProjectType-livingComfort a ff:AnswerOption ; rdfs:label "Wohnkomfort"@de , "Living comfort"@en .
ff:housingProjectType-burglaryProtection a ff:AnswerOption ; rdfs:label "Einbruchschutz"@de , "Burglary protection"@en .
ff:housingProjectType-other a ff:AnswerOption ; rdfs:label "Andere"@de , "Other"@en .

ff:buildingCountry a ff:DataField ;
    schema:category ff:wohnungsbau-df-kategorie ;
    rdfs:label "Gebäudestandort Land"@de , "Building Location Country"@en ;
    schema:question "In welchem Land befindet sich das Gebäude, das Sie sanieren oder nachrüsten möchten?"@de ,
                    "In which country is the building you want to renovate or retrofit located?"@en ;
    rdfs:comment "Erfasst wird das Land, in dem sich das zu sanierende oder nachzurüstende Gebäude befindet."@de ,
                 "This field captures the country in which the building to be renovated or retrofitted is located."@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:buildingCountry ;
        sh:property [
            sh:path ff:buildingCountry ;
            sh:in (
                ff:buildingCountry-ger
                ff:buildingCountry-eu
                ff:buildingCountry-other
            ) ;
            sh:maxCount 1 ;
        ] ;
    ] .
ff:buildingCountry-ger a ff:AnswerOption ; rdfs:label "Deutschland"@de , "Germany"@en .
ff:buildingCountry-eu a ff:AnswerOption ; rdfs:label "Europäische Union (außer Deutschland)"@de , "European Union (excluding Germany)"@en .
ff:buildingCountry-other a ff:AnswerOption ; rdfs:label "Anderes Land"@de , "Other country"@en .

ff:buildingTypeRenovation a ff:DataField ;
    schema:category ff:wohnungsbau-df-kategorie ;
    rdfs:label "Gebäudeart - Sanierung oder Neubau"@de , "Building Type - Renovation or New Construction"@en ;
    schema:question "Handelt es sich bei dem Gebäude, das Sie sanieren oder nachrüsten möchten, um ein bestehendes Gebäude oder ein Neubauprojekt?"@de ,
                    "Is the building you want to renovate or retrofit an existing building or a new construction project?"@en ;
    rdfs:comment "Die Frage unterscheidet, ob es sich bei dem geplanten Vorhaben um die Sanierung oder Nachrüstung eines bestehenden Gebäudes oder um ein Neubauprojekt handelt."@de ,
                 "This question distinguishes whether the planned project involves the renovation or retrofitting of an existing building or a new construction project."@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:buildingTypeRenovation ;
        sh:property [
            sh:path ff:buildingTypeRenovation ;
            sh:in (
                ff:buildingTypeRenovation-existing
                ff:buildingTypeRenovation-new
            ) ;
            sh:maxCount 1 ;
        ] ;
    ] .
ff:buildingTypeRenovation-existing a ff:AnswerOption ; rdfs:label "Bestehendes Gebäude"@de , "Existing building"@en .
ff:buildingTypeRenovation-new a ff:AnswerOption ; rdfs:label "Neubauprojekt"@de , "New construction project"@en .

ff:renovationType a ff:DataField ;
    schema:category ff:wohnungsbau-df-kategorie ;
    rdfs:label "Building Renovation Type"@en, "Art der Gebäudesanierung"@de ;
    schema:question "What type of renovation or retrofitting work do you plan to carry out on the building?"@en, "Welche Art von Sanierungs- oder Nachrüstungsarbeiten planen Sie am Gebäude durchzuführen?"@de ;
    rdfs:comment "The type of renovation or retrofitting work planned for the building can be relevant for funding opportunities that target specific energy-saving measures or building improvements. Some grants are specifically available for projects involving insulation upgrades, window replacements, or heating system modernization."@en,
                 "Die Art der geplanten Sanierungs- oder Nachrüstungsarbeiten am Gebäude kann für Fördermöglichkeiten relevant sein, die auf spezifische Energiesparmaßnahmen oder Gebäudeverbesserungen abzielen. Einige Förderungen sind speziell für Projekte verfügbar, die Dämmungsverbesserungen, Fensteraustausch oder die Modernisierung von Heizungssystemen umfassen."@de ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:renovationType ;
        sh:property [
            sh:path ff:renovationType ;
            sh:in (
                ff:renovationType-energyEfficiency
                ff:renovationType-renewableHeating
                ff:renovationType-other
            ) ;
            sh:maxCount 1 ;
        ] ;
    ] .
ff:renovationType-energyEfficiency a ff:AnswerOption ; rdfs:label "Energy efficiency improvements"@en, "Energieeffizienzverbesserungen"@de .
ff:renovationType-renewableHeating a ff:AnswerOption ; rdfs:label "Renewable heating systems"@en, "Erneuerbare Heizsysteme"@de .
ff:renovationType-other a ff:AnswerOption ; rdfs:label "Other"@en, "Sonstige"@de .

ff:companyFounded a ff:DataField ;
    schema:category ff:employmentDfKategorie ;
    rdfs:label "Unternehmensgründung"@de , "Company Founded"@en ;
    schema:question "Wurde Ihr Unternehmen bereits offiziell gegründet?"@de ,
                    "Has your company already been officially founded?"@en ;
    rdfs:comment "Erfasst wird, ob ein Unternehmen rechtlich gegründet wurde, also eine Eintragung im Handelsregister oder eine andere offizielle Registrierung erfolgt ist."@de ,
                 "This field captures whether a company has been legally established, meaning it has been registered in the commercial register or through another official registration."@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:companyFounded ;
        sh:property [
            sh:path ff:companyFounded ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;
    ] .

ff:compositionFoundingTeam a ff:DataField ;
    schema:category ff:gruendung-df-kategorie ;
    rdfs:label "Startup Team Composition"@en, "Zusammensetzung des Gründungsteams"@de ;
    schema:question "What is the educational and professional background of your startup team members?"@en, 
                    "Welchen Bildungs- und Berufsabschluss haben die Mitglieder Ihres Gründungsteams?"@de ;
    rdfs:comment "Some funding programs take the educational composition of founding teams into account, especially regarding the presence of academic versus non-academic backgrounds. This helps determine eligibility for grants aimed at research-based startups or those supporting founders without formal academic degrees."@en,
                 "Einige Förderprogramme berücksichtigen die Bildungszusammensetzung von Gründungsteams, insbesondere in Bezug auf akademische und nicht-akademische Hintergründe. Dies kann für Förderungen relevant sein, die sich gezielt an forschungsbasierte Startups oder Gründer ohne akademischen Abschluss richten."@de ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:compositionFoundingTeam ;
        sh:property [
            sh:path ff:compositionFoundingTeam ;
            sh:in (
                ff:compositionFoundingTeam-onlyAcademic
                ff:compositionFoundingTeam-oneNonAcademic
                ff:compositionFoundingTeam-moreThanOneNonAcademic
                ff:compositionFoundingTeam-atLeastOneNonDegree
            ) ;
            sh:maxCount 1 ;
        ] ;
    ] .
ff:compositionFoundingTeam-onlyAcademic a ff:AnswerOption ;
    rdfs:label "Only team members with academic degrees (e.g., bachelor's, master's, PhD)"@en, 
               "Nur Teammitglieder mit akademischen Abschlüssen (z. B. Bachelor, Master, Promotion)"@de .
ff:compositionFoundingTeam-oneNonAcademic a ff:AnswerOption ;
    rdfs:label "Mostly academic degrees, but at most one team member has a non-academic professional qualification"@en, 
               "Überwiegend akademische Abschlüsse, aber maximal ein Teammitglied mit nicht-akademischem Berufsabschluss"@de .
ff:compositionFoundingTeam-moreThanOneNonAcademic a ff:AnswerOption ;
    rdfs:label "More than one team member has a non-academic professional qualification (e.g., vocational training)"@en, 
               "Mehr als ein Teammitglied hat einen nicht-akademischen Berufsabschluss (z. B. Berufsausbildung)"@de .
ff:compositionFoundingTeam-atLeastOneNonDegree a ff:AnswerOption ;
    rdfs:label "At least one team member has no formal professional or academic qualification"@en, 
               "Mindestens ein Teammitglied hat keinen formalen Berufs- oder akademischen Abschluss"@de .

ff:majorityShares a ff:DataField ;
    schema:category ff:gruendung-df-kategorie ;
    rdfs:label "Majority Shares"@en, "Mehrheitliche Geschäftsanteile"@de ;
    schema:question "Do the founders hold more than 50% of the company shares and perform key management roles?"@en, "Halten die Gründer*innen mehr als 50% der Geschäftsanteile und übernehmen zentrale Managementaufgaben?"@de ;
    rdfs:comment "The distribution of shares among founders can be relevant for funding opportunities that require majority ownership or specific roles within the company. Some grants are specifically available for startups where the founders hold more than 50% of the shares and are actively involved in key management functions."@en,
                 "Die Verteilung der Geschäftsanteile unter den Gründer*innen kann für Fördermöglichkeiten relevant sein, die eine Mehrheitsbeteiligung oder spezifische Rollen innerhalb des Unternehmens voraussetzen. Einige Förderungen sind speziell für Startups verfügbar, bei denen die Gründer*innen mehr als 50% der Anteile halten und aktiv an zentralen Managementfunktionen beteiligt sind."@de ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:majorityShares ;
        sh:property [
            sh:path ff:majorityShares ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;
    ] .

ff:interestedInSuccession a ff:DataField ;
    schema:category ff:gruendung-df-kategorie ;
    rdfs:label "Interest in Business Succession"@en, "Interesse an Unternehmensnachfolge"@de ;
    schema:question "Are you interested in business succession or taking over an existing company?"@en, "Sind Sie an einer Unternehmensnachfolge oder der Übernahme eines bestehenden Unternehmens interessiert?"@de ;
    rdfs:comment "This question helps identify individuals or organizations interested in business succession or taking over existing companies, which may be eligible for funding opportunities or grants supporting the transfer of ownership."@en,
                 "Diese Frage hilft dabei, Personen oder Organisationen zu identifizieren, die an einer Unternehmensnachfolge oder der Übernahme bestehender Unternehmen interessiert sind und möglicherweise für Fördermöglichkeiten oder Zuschüsse zur Unterstützung des Eigentumsübergangs infrage kommen."@de ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:interestedInSuccession ;
        sh:property [
            sh:path ff:interestedInSuccession ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;
    ] .

ff:businessPlan a ff:DataField ;
    schema:category ff:arbeitswelt-df-kategorie ;
    rdfs:label "Geschäftsplan"@de , "Business plan"@en ;
    schema:question "Haben Sie einen Geschäftsplan erstellt?"@de , 
        "Haben Sie einen Plan für Ihr Geschäft aufgeschrieben?"@de-x-es ,
        "Have you created a business plan?"@en ;
    rdfs:comment "Ein Geschäftsplan ist ein Dokument, das die Ziele, Strategien und Rahmenbedingungen einer Unternehmung beschreibt und dabei insbesondere darlegt, wie die wirtschaftliche Tragfähigkeit des Vorhabens sichergestellt wird."@de ,
        "Ein Geschäftsplan ist ein Text. Darin steht: Welche Ziele Sie haben, wie Sie arbeiten wollen und wie Sie mit Ihrem Geschäft Geld verdienen können."@de-x-es ,
        "A business plan is a document that outlines the goals, strategies, and conditions of a business venture, particularly detailing how the economic viability of the project will be ensured."@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:businessPlan ;
        sh:property [
            sh:path ff:businessPlan ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;
    ] . 

ff:bankAccountGermany a ff:DataField ;
    schema:category ff:gruendung-df-kategorie ;
    rdfs:label "Bank Account in Germany"@en, "Bankkonto in Deutschland"@de ;
    schema:question "Does your startup have a bank account in Germany?"@en, 
                    "Hat Ihr Startup ein Bankkonto in Deutschland?"@de ;
    rdfs:comment "Some funding programs require startups to have a bank account in Germany to receive financial support or grants. This question helps determine eligibility for grants that are specifically available to companies with a German bank account."@en,
                    "Einige Förderprogramme setzen voraus, dass Startups ein Bankkonto in Deutschland haben, um finanzielle Unterstützung oder Fördermittel zu erhalten. Diese Frage hilft dabei, die Förderfähigkeit für Förderungen zu bestimmen, die speziell für Unternehmen mit einem deutschen Bankkonto verfügbar sind."@de ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:bankAccountGermany ;
        sh:property [
            sh:path ff:bankAccountGermany ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;
    ] .

ff:companyAge a ff:DataField ;
    schema:category ff:gruendung-df-kategorie ;
    rdfs:label "Company Age"@en, "Alter des Unternehmens"@de ;
    schema:question "When was your startup founded?"@en, 
                    "Wann wurde Ihr Unternehmen gegründet?"@de ;
    rdfs:comment "The age of your startup can be relevant for funding opportunities that target early-stage ventures or require companies to be founded within a specific timeframe. Some grants are specifically available for startups founded less than 18 months ago or for companies that are not yet established."@en,
                    "Das Alter Ihres Unternehmens kann für Fördermöglichkeiten relevant sein, die sich an junge Unternehmen richten oder voraussetzen, dass Unternehmen innerhalb eines bestimmten Zeitraums gegründet wurden. Einige Förderungen sind speziell für Startups verfügbar, die vor weniger als 18 Monaten gegründet wurden oder für Unternehmen, die noch nicht etabliert sind."@de ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:companyAge ;
        sh:property [
            sh:path ff:companyAge ;
            sh:in (
                ff:companyAge-below12Months
                ff:companyAge-below18Months
                ff:companyAge-below5Years
                ff:companyAge-greater5Years
                ff:companyAge-notFounded
            ) ;
            sh:maxCount 1 ;
        ] ;
    ] .
ff:companyAge-below12Months a ff:AnswerOption ; rdfs:label "Less than 12 months ago"@en, "Vor weniger als 12 Monaten"@de .
ff:companyAge-below18Months a ff:AnswerOption ; rdfs:label "Less than 18 months ago"@en, "Vor weniger als 18 Monaten"@de .
ff:companyAge-below5Years a ff:AnswerOption ; rdfs:label "More than 18 months ago"@en, "Vor mehr als 18 Monaten"@de .
ff:companyAge-greater5Years a ff:AnswerOption ; rdfs:label "More than 5 years ago"@en, "Vor mehr als 5 Jahren"@de .
ff:companyAge-notFounded a ff:AnswerOption ; rdfs:label "Not yet founded"@en, "Noch nicht gegründet"@de .    


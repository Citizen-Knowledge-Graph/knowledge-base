ff:profitOriented a ff:DataField ;
    schema:category ff:gruendung-df-kategorie ;
    rdfs:label "Legal Entity"@en, "Rechtsform des Unternehmens"@de ;
    schema:question "Is your company profit-oriented?"@en, "Ist Ihr Unternehmen gewinnorientiert?"@de ;

        a sh:NodeShape ;
        sh:targetSubjectsOf ff:deMinimisAid ;
        sh:property [
            sh:path ff:deMinimisAid ;
            sh:in (
                ff:deMinimisAid-no
                ff:deMinimisAid-yes
                ff:deMinimisAid-unsure
            ) ;
            sh:maxCount 1 ;
        ] ;
    ] .

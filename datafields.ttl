@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ff: <https://foerderfunke.org/default#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .

# Persönliche Angaben

ff:geburtsdatum a ff:DataField ;
    schema:category ff:persoenliche-angaben-df-kategorie;
    rdfs:label "Geburtsdatum"@de , "Date of Birth"@en ;
    schema:question "Wann sind Sie Geburtsdatum?"@de , "What is your date of birth?"@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:geburtsdatum ;
        sh:property [
            sh:path ff:geburtsdatum ;
            sh:datatype xsd:date ;
            sh:maxCount 1 ;
        ] ;
    ] .

ff:staatsbuergerschaft a ff:DataField ;
    schema:category ff:persoenliche-angaben-df-kategorie;
    rdfs:label "Staatsbürgerschaft"@de , "Citizenship"@en ;
    schema:question "Welche Staatsbürgerschaft haben Sie?"@de , "What is your citizenship?"@en ;
    rdfs:comment "Welches Land steht auf Ihrem Pass oder Ausweis? Meistens ist das Deutschland. Mitgliedstaaten der Europäischen Union sind: Belgien, Bulgarien, Dänemark, Deutschland, Estland, Finnland, Frankreich, Griechenland, Irland, Italien, Kroatien, Lettland, Litauen, Luxemburg, Malta, die Niederlande, Österreich, Polen, Portugal, Rumänien, Schweden, die Slowakei, Slowenien, Spanien, die Tschechische Republik, Ungarn und Zypern."@de ,
        "Which country is on your passport or ID card? Mostly it is Germany. Member states of the European Union are: Austria, Belgium, Bulgaria, Croatia, Cyprus, Czech Republic, Denmark, Estonia, Finland, France, Germany, Greece, Hungary, Ireland, Italy, Latvia, Lithuania, Luxembourg, Malta, Netherlands, Poland, Portugal, Romania, Slovakia, Slovenia, Spain, Sweden."@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:staatsbuergerschaft ;
        sh:property [
            sh:path ff:staatsbuergerschaft ;
            sh:in (ff:staatsbuergerschaft-ao-ger ff:staatsbuergerschaft-ao-eu ff:staatsbuergerschaft-ao-3rd) ;
            sh:maxCount 1 ;
        ] ;
    ] .
ff:staatsbuergerschaft-ao-ger a ff:AnswerOption ; rdfs:label "Deutsch"@de , "German"@en .
ff:staatsbuergerschaft-ao-eu a ff:AnswerOption ; rdfs:label "EU"@de , "EU"@en .
ff:staatsbuergerschaft-ao-3rd a ff:AnswerOption ; rdfs:label "Drittstaat (Nicht-EU)"@de , "Third country (non-EU)"@en .

ff:aufenthalt a ff:DataField ;
    schema:category ff:persoenliche-angaben-df-kategorie;
    rdfs:label "Aufenthaltsberechtigung"@de , "Right of residence"@en ;
    schema:question "Sind Sie in Deutschland aufenthaltsberechtigt?"@de , "Are you entitled to stay in Germany?"@en ;
    rdfs:comment "Wenn Sie Ausländer:in sind und eine Aufenthaltsberechtigung haben (diese wurden zwischen 1991 und 2004 ausgestellt) oder eine Niederlassungsberechtigung (seit 2005), klicken Sie „ja“ an. Wenn Sie das nicht haben, klicken Sie „nein“ an."@de ,
        "If you are a foreigner and have a residence permit (these were issued between 1991 and 2004) or a settlement permit (since 2005), click \"yes\". If you do not have one, click \"no\"."@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:aufenthalt ;
        sh:property [
            sh:path ff:aufenthalt ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;
    ] .

ff:aufenthaltsort a ff:DataField ;
    schema:category ff:persoenliche-angaben-df-kategorie;
    rdfs:label "Aufenthaltsort"@de , "Actual Residence"@en ;
    schema:question "Wo ist Ihr tatsächlicher Aufenthaltsort, an dem Sie erreichbar sind?"@de ,
        "Where is your actual residence where you can be reached?"@en ;
    rdfs:comment "Das ist der Ort, wo Sie sich wirklich aufhalten. Es ist egal, ob Sie dort gemeldet sind oder das Recht haben, hier zu leben. Das kann auch eine Pflegeeinrichtung, ein Frauenhaus, ein Stadtteil oder Ähnliches sein."@de ,
        "This is the place where you are actually staying. It does not matter whether you are registered there or have the right to live here. This can also be a care facility, a women's shelter, a city district or the like."@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:aufenthaltsort ;
        sh:property [
            sh:path ff:aufenthaltsort ;
            sh:in (ff:aufenthaltsort-ao-ausserhalb ff:aufenthaltsort-ao-innerhalb) ;
            sh:maxCount 1 ;
        ] ;
    ] .
ff:aufenthaltsort-ao-ausserhalb a ff:AnswerOption ; rdfs:label "Außerhalb Deutschlands"@de , "Outside Germany"@en .
ff:aufenthaltsort-ao-innerhalb a ff:AnswerOption ; rdfs:label "Innerhalb Deutschlands"@de , "Inside Germany"@en .

ff:household_members a ff:DataField ;
    schema:category ff:persoenliche-angaben-df-kategorie;
    rdfs:label "Haushaltsmitglieder"@de , "Household Members"@en ;
    schema:question "Wie viele Personen leben in Ihrem Haushalt?"@de , "How many people live in your household?"@en ;
    rdfs:comment "Zählen Sie alle Personen, die in Ihrer Wohnung oder Ihrem Haus leben. Dazu gehören auch Kinder, die bei Ihnen leben, aber nicht Ihr eigenes sind. Zählen Sie auch Personen, die nicht arbeiten, wie zum Beispiel Rentner:innen oder Kinder."@de ,
        "Count all the people who live in your apartment or house. This includes children who live with you but are not your own. Also count people who do not work, such as pensioners or children."@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:household_members ;
        sh:property [
            sh:path ff:household_members ;
            sh:datatype xsd:integer ;
            sh:maxCount 1 ;
        ] ;
    ] .

# Kinder

ff:kinder a ff:DataField ;
    schema:category ff:kinder-df-kategorie ;
    rdfs:label "Kinder"@de , "Children"@en ;
    schema:question "Haben Sie ein oder mehrere Kinder?"@de , "Do you have one or more children?"@en ;
    rdfs:comment "Bitte geben Sie an, ob Kinder in Ihrem Haushalt leben."@de ,
        "Please specify if there are children living in your household."@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:kinder ;
        sh:property [
            sh:path ff:kinder ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;
    ] .

ff:kinder_unter_18 a ff:DataField ;
    schema:category ff:kinder-df-kategorie ;
    rdfs:label "Kinder unter 18"@de , "Children under 18"@en ;
    schema:question "Haben Sie Kinder unter 18 Jahren?"@de , "Do you have children younger than 18 years old?"@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:kinder_unter_18 ;
        sh:property [
            sh:path ff:kinder_unter_18 ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;
    ] .

ff:kinder_18_25 a ff:DataField ;
    schema:category ff:kinder-df-kategorie ;
    rdfs:label "Kinder 18-25"@de , "Children 18-25"@en ;
    schema:question "Haben Sie Kinder zwischen 18-25 Jahren?"@de , "Do you have children between the ages of 18 and 25?"@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:kinder_18_25 ;
        sh:property [
            sh:path ff:kinder_18_25 ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;
    ] .

ff:kinder_in_ausbildung a ff:DataField ;
    schema:category ff:kinder-df-kategorie ;
    rdfs:label "Kinder in Ausbildung"@de , "Children in Education"@en ;
    schema:question "Haben Sie Kinder zwischen 18 und 25 Jahren, die sich noch in Schule, Ausbildung oder Studium befinden?"@de ,
        "Do you have children between the ages of 18 and 25 who are still in school, training or studying?"@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:kinder_in_ausbildung ;
        sh:property [
            sh:path ff:kinder_in_ausbildung ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;
    ] .

ff:receiving_kindergeld a ff:DataField ;
    schema:category ff:kinder-df-kategorie ;
    rdfs:label "Kindergeld"@de , "Child Benefit"@en ;
    schema:question "Beziehen Sie Kindergeld für ein oder mehrere Kinder?"@de ,
        "Do you receive child benefit for one or more children?"@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:receiving_kindergeld ;
        sh:property [
            sh:path ff:receiving_kindergeld ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;
    ] .

ff:hasChild a ff:DataField ;
    schema:category ff:persoenliche-angaben-df-kategorie;
    rdfs:label "Anzahl Kinder"@de , "Number of children"@en ;
    schema:question "Wie viele Kinder haben Sie?"@de , "How many children do you have?"@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:hasChild ;
        sh:property [
            sh:path ff:hasChild ;
            sh:class ff:Child ;
        ] ;
    ] .

# Erwerbstätigkeit

ff:erwerbsfaehig_neu a ff:DataField ;
    schema:category ff:erwerbstaetigkeit-df-kategorie ;
    rdfs:label "Erwerbsfähig"@de , "Fit for Work"@en ;
    schema:question "Sind Sie erwerbsfähig und können täglich mindestens 3 Stunden arbeiten?"@de ,
        "Are you fit for work and able to work at least 3 hours a day?"@en ;
    rdfs:comment "Sie sind erwerbsfähig, wenn Sie mindestens 3 Stunden am Tag normal arbeiten können. Wenn Sie es wegen längerer Krankheit oder Behinderung nicht schaffen, so lange zu arbeiten, geben Sie „nein“ an."@de ,
        "You are fit for work if you can work normally for at least 3 hours a day. If you cannot manage to work that long because of a prolonged illness or disability, click “no”."@en ;
    ff:hasShaclShape [
    a sh:NodeShape ;
        sh:targetSubjectsOf ff:erwerbsfaehig_neu ;
        sh:property [
            sh:path ff:erwerbsfaehig_neu ;
            sh:in (ff:erwerbsfaehig_neu-ao-ja ff:erwerbsfaehig_neu-ao-nein-ueber-6-monate ff:erwerbsfaehig_neu-ao-nein-weniger-6-monate) ;
            sh:maxCount 1 ;
        ] ;
    ] .
ff:erwerbsfaehig_neu-ao-ja a ff:AnswerOption ; rdfs:label "ja"@de , "yes"@en .
ff:erwerbsfaehig_neu-ao-nein-ueber-6-monate a ff:AnswerOption ; rdfs:label "Nein, dauerhaft erwerbsunfähig (über 6 Monate)"@de , "No, permanently unable to work (more than 6 months)"@en .
ff:erwerbsfaehig_neu-ao-nein-weniger-6-monate a ff:AnswerOption ; rdfs:label "Nein, temporär erwerbsunfähig (weniger als 6 Monate)"@de , "No, temporarily unable to work (less than 6 months)"@en .

ff:beruf_neu a ff:DataField ;
    schema:category ff:erwerbstaetigkeit-df-kategorie ;
    rdfs:label "Beruf"@de , "Profession"@en ;
    schema:question "Wie ist Ihre berufliche Situation?"@de , "What is your professional situation?"@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:beruf_neu ;
        sh:property [
            sh:path ff:beruf_neu ;
            sh:in (ff:beruf_neu-ao-selbstaendig ff:beruf_neu-ao-soz ff:beruf_neu-ao-alg ff:beruf_neu-ao-ausbildung ff:beruf_neu-ao-bfd ff:beruf_neu-ao-rente ff:beruf_neu-ao-eltern) ;
            sh:maxCount 1 ;
        ] ;
    ] .
ff:beruf_neu-ao-selbstaendig a ff:AnswerOption ; rdfs:label "Selbständig in einer Beschäftigung von mehr als 15 Stunden pro Woche"@de , "Self-employed, working more than 15 hours per week"@en .
ff:beruf_neu-ao-soz a ff:AnswerOption ; rdfs:label "Berufstätig in einer sozialversicherungspflichtigen Beschäftigung"@de , "Employed in a job subject to social security contributions"@en .
ff:beruf_neu-ao-alg a ff:AnswerOption ; rdfs:label "Arbeitslos"@de , "Unemployed"@en .
ff:beruf_neu-ao-ausbildung a ff:AnswerOption ; rdfs:label "Schule/Ausbildung/Studium"@de , "School/vocational training/studies"@en .
ff:beruf_neu-ao-bfd a ff:AnswerOption ; rdfs:label "Bundesfreiwilligendienst"@de , "Federal voluntary service"@en .
ff:beruf_neu-ao-rente a ff:AnswerOption ; rdfs:label "Regelaltersrente/Pension"@de , "Standard old-age pension/retirement"@en .
ff:beruf_neu-ao-eltern a ff:AnswerOption ; rdfs:label "Elternzeit"@de , "Parental leave"@en .

ff:einkommen_neu a ff:DataField ;
    schema:category ff:erwerbstaetigkeit-df-kategorie ;
    rdfs:label "Einkommen"@de , "Income"@en ;
    schema:question "Wie erzielen Sie Ihr Einkommen? (Mehrfachnennung möglich)"@de ,
        "How do you earn your income? (multiple answers possible)"@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:einkommen_neu ;
        sh:property [
            sh:path ff:einkommen_neu ;
            sh:in (ff:einkommen_neu-ao-selbstaendig ff:einkommen_neu-ao-soz ff:einkommen_neu-ao-alg ff:einkommen_neu-ao-alg-ii ff:einkommen_neu-ao-ausbildung ff:einkommen_neu-ao-bafoeg ff:einkommen_neu-ao-rente ff:einkommen_neu-ao-elterngeld ff:einkommen_neu-ao-mutterschaftsgeld ff:einkommen_neu-ao-wohngeld ff:einkommen_neu-ao-sonstige) ;
        ] ;
    ] .
ff:einkommen_neu-ao-selbstaendig a ff:AnswerOption ; rdfs:label "Selbstständige Arbeit in einer Beschäftigung von mehr als 15 Stunden pro Woche"@de , "Self-employed work of more than 15 hours per week"@en .
ff:einkommen_neu-ao-soz a ff:AnswerOption ; rdfs:label "Sozialversicherungspflichtige Beschäftigung"@de , "Employment subject to social security contributions"@en .
ff:einkommen_neu-ao-ausbildung a ff:AnswerOption ; rdfs:label "Ausbildungsvergütung"@de , "Training allowance"@en .
ff:einkommen_neu-ao-sonstige a ff:AnswerOption ; rdfs:label "Sonstige"@de , "Other"@en .

ff:einkommen_familie a ff:DataField ;
    schema:category ff:erwerbstaetigkeit-df-kategorie ;
    rdfs:label "Einkommen Familie"@de , "Family Income"@en ;
    schema:question "Wie beschreiben Sie das Einkommen Ihrer Familie?"@de , "How do you describe your family's income?"@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:einkommen_familie ;
        sh:property [
            sh:path ff:einkommen_familie ;
            sh:in (ff:einkommen_familie-ao-monatsbrutto-ueber-900-600 ff:einkommen_familie-ao-monatsbrutto-unter-900-600) ;
            sh:maxCount 1 ;
        ] ;
    ] .
ff:einkommen_familie-ao-monatsbrutto-ueber-900-600 a ff:AnswerOption ; rdfs:label "Die Familie hat ein monatliches Brutto-Einkommen von mindestens 900 Euro (Gemeinsam erziehend) bzw. 600 Euro (Alleinerziehende)"@de , "The family has a monthly gross income of at least 900 euros (joint parents) or 600 euros (single parents)."@en .
ff:einkommen_familie-ao-monatsbrutto-unter-900-600 a ff:AnswerOption ; rdfs:label "Die Familie hat kein monatliches Brutto-Einkommen von mindestens 900 Euro (Gemeinsam erziehend) bzw. 600 Euro (Alleinerziehende)"@de , "The family does not have a monthly gross income of at least 900 euros (joint parents) or 600 euros (single parents)."@en .

# Lebenssituation

ff:lebenssituation a ff:DataField ;
    schema:category ff:lebenssituation-df-kategorie ;
    rdfs:label "Lebenssituation"@de , "Living Situation"@en ;
    schema:question "Wie ist Ihre Lebenssituation?"@de , "What is your living situation?"@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:lebenssituation ;
        sh:property [
            sh:path ff:lebenssituation ;
            sh:in (ff:lebenssituation-ao-allein ff:lebenssituation-ao-gemeinschaft ff:lebenssituation-ao-allein-mit-kind) ;
            sh:maxCount 1 ;
        ] ;
    ] .
ff:lebenssituation-ao-allein a ff:AnswerOption ; rdfs:label "Alleinlebend"@de , "Single household"@en .
ff:lebenssituation-ao-gemeinschaft a ff:AnswerOption ; rdfs:label "Gemeinsam lebend (Ehe, Partnerschaft, eingetragene Lebenspartnerschaft), ggf. mit Kindern"@de , "Joint household (marriage, partnership, registered partnership), possibly with children"@en .
ff:lebenssituation-ao-allein-mit-kind a ff:AnswerOption ; rdfs:label "Alleinerziehend mit mind. einem Kind"@de , "Single parent with at least one child"@en .

ff:lebenslage a ff:DataField ;
    schema:category ff:lebenssituation-df-kategorie ;
    rdfs:label "Lebenslage"@de , "Living Situation"@en ;
    schema:question "Befinden Sie sich in einer sozial schwierigen Lebenslage?"@de ,
        "Are you in a difficult social situation?"@en ;
    rdfs:comment "Schwierige Lebenslagen oder Notsituationen sind zum Beispiel: Sie haben keine Wohnung mehr (Obdachlosigkeit), Sie haben ein Suchtproblem, Sie wissen nicht, wovon Sie leben sollen oder wurden ein Opfer von Gewalt in der Familie."@de ,
        "Difficult living situations or emergency situations are, for example: you no longer have a home (homelessness), you have an addiction problem, you do not know how to cover your living expenses, or you have been a victim of violence in the family."@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:lebenslage ;
        sh:property [
            sh:path ff:lebenslage ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;
    ] .

ff:vermoegen a ff:DataField ;
    schema:category ff:lebenssituation-df-kategorie ;
    rdfs:label "Vermögen"@de , "Assets"@en ;
    schema:question "Wie hoch ist Ihr Vermögen? Runden Sie auf 5.000 Euro auf."@de , "What is the value of your assets? Round up to 5,000 euros."@en ;
    rdfs:comment "Vermögen sind zum Beispiel: Bargeld, Sparguthaben, Wertpapiere, Immobilien, Schmuck, Kunstgegenstände, Autos, Boote, Anteile an Unternehmen oder Lebensversicherungen."@de ,
        "Assets include: cash, savings, securities, real estate, jewelry, works of art, cars, boats, shares in companies or life insurance."@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:vermoegen ;
        sh:property [
            sh:path ff:vermoegen ;
            sh:datatype xsd:integer ;
            sh:maxCount 1 ;
        ] ;
    ] .

ff:experience_financial_difficulties a ff:DataField ;
    schema:category ff:lebenssituation-df-kategorie ;
    rdfs:label "Finanzielle Schwierigkeiten"@de , "Financial Difficulties"@en ;
    schema:question "Haben Sie finanzielle Schwierigkeiten, Ihren oder den Lebensunterhalt Ihres Haushalts zu bewältigen?"@de ,
        "Do you have financial difficulties in managing your or your household's livelihood?"@en ;
    rdfs:comment "Einige Leistungen setzen voraus, dass Sie in einer finanziellen Notlage sind. Das bedeutet, dass Sie nicht genug Geld haben, um Ihre Lebenshaltungskosten zu decken. Wenn Sie zum Beispiel Schulden haben, Ihre Miete nicht bezahlen können, Ihre Rechnungen nicht bezahlen können oder Ihre Lebenshaltungskosten nicht bezahlen können. Teil eines entsprechenden Antragsverfahrens ist die Prüfung, ob Sie in einer finanziellen Notlage sind."@de ,
        "Some benefits require that you are in a financial emergency. This means that you do not have enough money to cover your living expenses. For example, if you have debts, cannot pay your rent, cannot pay your bills, or cannot pay your living expenses. Part of the corresponding application process is the examination of whether you are in a financial emergency."@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:experience_financial_difficulties ;
        sh:property [
            sh:path ff:experience_financial_difficulties ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;
    ] .

# Gesundheit

ff:krankenversicherung a ff:DataField ;
    schema:category ff:gesundheit-df-kategorie ;
    rdfs:label "Krankenversicherung"@de , "Health Insurance"@en ;
    schema:question "Haben Sie eine Krankenversicherung?"@de , "Do you have health insurance?"@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:krankenversicherung ;
        sh:property [
            sh:path ff:krankenversicherung ;
            sh:in (
                ff:krankenversicherung-yes
                ff:krankenversicherung-no
                ff:krankenversicherung-unsure
            )
        ] ;
    ] .
ff:krankenversicherung-yes a ff:AnswerOption ; rdfs:label "Ja"@de , "Yes"@en .
ff:krankenversicherung-no a ff:AnswerOption ; rdfs:label "Nein"@de , "No"@en .
ff:krankenversicherung-unsure a ff:AnswerOption ; rdfs:label "Nicht sicher"@de , "Not sure"@en .


ff:pflegeversicherung a ff:DataField ;
    schema:category ff:gesundheit-df-kategorie ;
    rdfs:label "Pflegeversicherung"@de , "Nursing Care Insurance"@en ;
    schema:question "Haben Sie eine Pflegeversicherung?"@de , "Do you have nursing care insurance?"@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:pflegeversicherung ;
        sh:property [
            sh:path ff:pflegeversicherung ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;
    ] .

ff:pflege a ff:DataField ;
    schema:category ff:gesundheit-df-kategorie ;
    rdfs:label "Pflege"@de , "Care"@en ;
    schema:question "Sind Sie pflegebedürftig?"@de , "Are you in need of care?"@en ;
    rdfs:comment "Die Pflegebedürftigkeit wird von einem Gutachter des Medizinischen Dienstes der Krankenversicherung (MDK) festgestellt. Nach der Prüfung entscheidet die Pflegekasse über Pflegebedürftigkeit und Pflegegrad."@de ,
        "The need for care is determined by an expert from the health insurance's medical service (MDK). After the examination, the care fund (Pflegekasse) decides on the need for care and the degree of care."@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:pflege ;
        sh:property [
            sh:path ff:pflege ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;
    ] .

ff:care_recipient_close_relative a ff:DataField ;
    schema:category ff:gesundheit-df-kategorie ;
    rdfs:label "Pflegebedürftiger naher Angehöriger"@de , "Care recipient close relative"@en ;
    schema:question "Ist die pflegebedürftige Person eine nahe Angehörige oder Angehöriger?"@de ,
        "Is the person in need of care a close relative?"@en ;
    rdfs:comment "Zu „nahen Angehörigen“ zählen Großeltern, Eltern, Schwieger- und Stiefeltern, Ehegatten, Lebenspartner oder Partner einer ehe- bzw. lebenspartnerschaftsähnlichen Gemeinschaft, Geschwister sowie deren Ehe- oder Lebenspartner, Kinder, Adoptiv-, Pflege- und Stiefkinder, Schwiegerkinder und Enkelkinder."@de ,
        "Close relatives include grandparents, parents, parents-in-law and step-parents, spouses, life partners or partners in a marriage or life partnership-like community, siblings and their spouses or life partners, children, adopted, foster and step-children, children-in-law and grandchildren."@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:care_recipient_close_relative ;
        sh:property [
            sh:path ff:care_recipient_close_relative ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;
    ] .    

ff:acute_care_situation a ff:DataField ;
    schema:category ff:gesundheit-df-kategorie ;
    rdfs:label "Akute Pflegesituation"@de , "Acute Care Situation"@en ;
    schema:question "Befindet sich eine Person in Ihrem Umfeld in einer akuten Pflegesituation?"@de ,
        "Is there a person in your environment in an acute care situation?"@en ;
    rdfs:comment "Eine akute Pflegesituation liegt vor, wenn bei einer Person unerwartet ein Pflegebedarf eintritt (z. B. durch Unfall oder plötzlichen Krankheitsschub), sodass unverzüglich Pflege organisiert oder selbst geleistet werden muss und dafür sofortiger persönlicher Einsatz erforderlich ist."@de ,
        "An acute care situation exists when a person unexpectedly requires care (e.g. due to an accident or sudden illness), so that care must be organized or provided immediately and immediate personal commitment is required."@en ;        
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:acute_care_situation ;
        sh:property [
            sh:path ff:acute_care_situation ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;
    ] .

ff:required_for_care a ff:DataField ;
    schema:category ff:gesundheit-df-kategorie ;
    rdfs:label "Erforderlich für Pflege"@de , "Required for Care"@en ;
    schema:question "Ist Ihre persönliche Mitwirkung erforderlich, um die Pflege der betroffenen Person sicherzustellen?"@de ,
        "Is your personal involvement required to ensure the care of the affected person?"@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:required_for_care ;
        sh:property [
            sh:path ff:required_for_care ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;
    ] .

# Sozialleistungen

ff:bezogene_leistungen a ff:DataField ;
    schema:category ff:sozialleistungen-df-kategorie ;
    rdfs:label "Bezogene Leistungen"@de , "Received Benefits"@en ;
    schema:question "Erhalten Sie eine oder mehrere der folgenden Leistungen? (Mehrfachnennung möglich)"@de, "Do you receive one or more of the following benefits? (multiple answers possible)"@en ;
    rdfs:comment "Teilweise schließen sich bestimmte Leistungen gegenseitig aus. Wenn Sie beispielsweise Wohngeld beziehen, können Sie kein Bürgergeld erhalten. Auf der anderen Seite sind einige Leistungen Voraussetzung für andere."@de ,
        "Some benefits are mutually exclusive. For example, if you receive housing benefit, you cannot receive citizen's income. On the other hand, some benefits are a prerequisite for others."@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:sozialleistungen ;
        sh:property [
            sh:path ff:sozialleistungen ;
            sh:in (
                ff:bezogene_leistungen-keine
                ff:bezogene_leistungen-arbeitslosengeld
                ff:bezogene_leistungen-buergergeld
                ff:bezogene_leistungen-asylblg
                ff:bezogene_leistungen-bafoeg
                ff:bezogene_leistungen-wohngeld
                ff:bezogene_leistungen-kinderzuschlag
                ff:bezogene_leistungen-elterngeld
                ff:bezogene_leistungen-rente
                ff:bezogene_leistungen-mutterschaftsgeld
                ff:bezogene_leistungen-erwerbsminderungsrente
                ff:bezogene_leistungen-grundsicherung
                ff:bezogene_leistungen-hilfe-zum-lebensunterhalt
                ff:bezogene_leistungen-berufsausbildungsbeihilfe-und-ausbildungsgeld
                ff:bezogene_leistungen-sicherung-des-lebensunterhaltes-mobipro-EU
            ) ;
        ] ;
    ] .
ff:bezogene_leistungen-keine a ff:AnswerOption ; rdfs:label "Keine"@de , "None"@en .
ff:bezogene_leistungen-arbeitslosengeld a ff:AnswerOption ; rdfs:label "Arbeitslosengeld"@de , "Unemployment benefit"@en .
ff:bezogene_leistungen-buergergeld a ff:AnswerOption ; rdfs:label "Bürgergeld (früher ALG II)"@de , "Citizen's income (formerly unemployment benefit II)"@en .
ff:bezogene_leistungen-asylblg a ff:AnswerOption ; rdfs:label "Asylbewerberleistungen (AsylbLG)"@de , "Asylum seeker benefits (AsylbLG)"@en .
ff:bezogene_leistungen-bafoeg a ff:AnswerOption ; rdfs:label "BAföG"@de , "Student support (BAföG)"@en .
ff:bezogene_leistungen-wohngeld a ff:AnswerOption ; rdfs:label "Wohngeld"@de , "Housing benefit"@en .
ff:bezogene_leistungen-kinderzuschlag a ff:AnswerOption ; rdfs:label "Kinderzuschlag"@de , "Child supplement"@en .
ff:bezogene_leistungen-elterngeld a ff:AnswerOption ; rdfs:label "Elterngeld"@de , "Parental allowance"@en .
ff:bezogene_leistungen-rente a ff:AnswerOption ; rdfs:label "Regelaltersrente"@de , "Standard retirement pension"@en .
ff:bezogene_leistungen-mutterschaftsgeld a ff:AnswerOption ; rdfs:label "Mutterschaftsgeld"@de , "Maternity pay"@en .
ff:bezogene_leistungen-erwerbsminderungsrente a ff:AnswerOption ; rdfs:label "Erwerbsminderungsrente"@de , "Disability pension"@en .
ff:bezogene_leistungen-grundsicherung a ff:AnswerOption ; rdfs:label "Grundsicherung im Alter und bei Erwerbsminderung"@de , "Basic security in old age and in case of reduced earning capacity"@en .
ff:bezogene_leistungen-hilfe-zum-lebensunterhalt a ff:AnswerOption ; rdfs:label "Hilfe zum Lebensunterhalt"@de , "Assistance for subsistence"@en .
ff:bezogene_leistungen-berufsausbildungsbeihilfe-und-ausbildungsgeld a ff:AnswerOption ; rdfs:label "Berufsausbildungsbeihilfe und Ausbildungsgeld"@de , "Vocational training allowance and training allowance"@en .
ff:bezogene_leistungen-sicherung-des-lebensunterhaltes-mobipro-EU a ff:AnswerOption ; rdfs:label "Sicherung des Lebensunterhaltes im Rahmen des MobiPro-EU Programms"@de , "Securing the livelihood within the framework of the MobiPro-EU program"@en .

ff:buergergeld_vergangenheit a ff:DataField ;
    schema:category ff:sozialleistungen-df-kategorie ;
    rdfs:label "Bezogenes Bürgergeld in der Vergangenheit"@de , "Received citizen's income in the past"@en ;
    schema:question "Haben Sie innerhalb der letzten 3 Jahre Bürgergeld bezogen?"@de , "Have you received citizen's income in the last 3 years?"@en ;
    rdfs:comment "Manche Leistungen oder Voraussetzungen für Leistungen hängen davon ab, ob Sie in der Vergangenheit Bürgergeld bezogen haben."@de ,
        "Some benefits or requirements for benefits depend on whether you have received citizen's income in the past."@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:buergergeld_vergangenheit ;
        sh:property [
            sh:path ff:buergergeld_vergangenheit ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;
    ] .

##############################
# Updated Graduation Status field for EXIST Gründerstipendium
##############################

ff:interested_in_founding a ff:DataField ;
    schema:category ff:gruendung-df-kategorie ;
    rdfs:label "Interested in Founding"@en, "Interesse an Gründung"@de ;
    schema:question "Are you interested in founding a company?"@en, "Sind Sie an der Gründung eines Unternehmens interessiert?"@de ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:interested_in_founding ;
        sh:property [
            sh:path ff:interested_in_founding ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;
    ] .

ff:graduation_status a ff:DataField ;
    schema:category ff:education-df-kategorie ;
    rdfs:label "University or Research Institution Status"@en, "Universitäts- oder Forschungseinrichtungsstatus"@de ;
    schema:question "Are you currently enrolled at a university or working at a research institution?"@en, 
                    "Sind Sie derzeit an einer Universität eingeschrieben oder arbeiten Sie an einer Forschungseinrichtung?"@de ;
    rdfs:comment "Some grants require applicants to be currently affiliated with a university or research institution or to have been in the past."@en,
                 "Einige Förderprogramme setzen voraus, dass Antragstellende aktuell mit einer Universität oder Forschungseinrichtung verbunden sind oder dies in der Vergangenheit waren."@de ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:graduation_status ;
        sh:property [
            sh:path ff:graduation_status ;
            sh:in (
                ff:graduation_status-currently-enrolled-or-working
                ff:graduation_status-left-at-most-5-years-ago
                ff:graduation_status-left-more-than-5-years-ago
                ff:graduation_status-never-enrolled
            ) ;
            sh:maxCount 1 ;
        ] ;
    ] .
ff:graduation_status-currently-enrolled-or-working a ff:AnswerOption ; 
    rdfs:label "I am currently enrolled at a university or working at a research institution"@en, 
               "Ich bin derzeit an einer Universität eingeschrieben oder arbeite an einer Forschungseinrichtung"@de .
ff:graduation_status-left-at-most-5-years-ago a ff:AnswerOption ; 
    rdfs:label "I left my university or research institution within the last 5 years"@en, 
               "Ich habe meine Universität oder Forschungseinrichtung innerhalb der letzten 5 Jahre verlassen"@de .
ff:graduation_status-left-more-than-5-years-ago a ff:AnswerOption ; 
    rdfs:label "I left my university or research institution more than 5 years ago"@en, 
               "Ich habe meine Universität oder Forschungseinrichtung vor mehr als 5 Jahren verlassen"@de .
ff:graduation_status-never-enrolled a ff:AnswerOption ; 
    rdfs:label "I was never enrolled at a university or employed at a research institution"@en, 
               "Ich war nie an einer Universität eingeschrieben oder an einer Forschungseinrichtung beschäftigt"@de .

ff:institute_location a ff:DataField ;
    schema:category ff:education-df-kategorie ;
    rdfs:label "University or Research Institution Location"@en, "Standort der Universität oder Forschungseinrichtung"@de ;
    schema:question "Where is your university or research institution located?"@en, 
                    "Wo befindet sich Ihre Universität oder Forschungseinrichtung?"@de ;
    rdfs:comment "Some grants are specifically available for individuals affiliated with universities or research institutions in Germany."@en,
                 "Einige Förderprogramme richten sich speziell an Personen, die mit Universitäten oder Forschungseinrichtungen in Deutschland verbunden sind."@de ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:institute_location ;
        sh:property [
            sh:path ff:institute_location ;
            sh:in ( ff:institute_location-germany ff:institute_location-outside-germany ) ;
            sh:maxCount 1 ;
        ] ;
    ] .
ff:institute_location-germany a ff:AnswerOption ; 
    rdfs:label "My university or research institution is located in Germany"@en, 
               "Meine Universität oder Forschungseinrichtung befindet sich in Deutschland"@de .
ff:institute_location-outside-germany a ff:AnswerOption ; 
    rdfs:label "My university or research institution is located outside Germany"@en, 
               "Meine Universität oder Forschungseinrichtung befindet sich außerhalb Deutschlands"@de .

ff:scholarship a ff:DataField ;
    schema:category ff:education-df-kategorie ;
    rdfs:label "Scholarship"@en, "Stipendium"@de ;
    schema:question "Do you currently receive a scholarship?"@en, 
                    "Erhalten Sie derzeit ein Stipendium?"@de ;
    rdfs:comment "This question includes all types of scholarships. However, some grants specifically consider scholarships that provide financial support for daily living expenses, such as rent, food, and other personal costs."@en,
                 "Diese Frage umfasst alle Arten von Stipendien. Einige Förderprogramme berücksichtigen jedoch speziell Stipendien, die finanzielle Unterstützung für die täglichen Lebenshaltungskosten wie Miete, Lebensmittel und sonstige persönliche Ausgaben bieten."@de ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:scholarship ;
        sh:property [
            sh:path ff:scholarship ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;
    ] .

ff:founding_status a ff:DataField ;
    schema:category ff:gruendung-df-kategorie ;
    rdfs:label "Startup Founding Status"@en, "Gründungsstatus"@de ;
    schema:question "Has your startup already been legally founded?"@en, 
                    "Wurde Ihr Unternehmen bereits offiziell gegründet?"@de ;
    rdfs:comment "This question refers to whether your startup has been legally registered as a business entity (e.g., GmbH, UG, GbR, or other legal forms). Some funding opportunities are specifically designed for newly founded startups, while others also support teams in the pre-founding phase."@en,
                 "Diese Frage bezieht sich darauf, ob Ihr Unternehmen bereits offiziell als juristische Person registriert wurde (z. B. GmbH, UG, GbR oder andere Rechtsformen). Einige Förderprogramme richten sich speziell an neu gegründete Startups, während andere auch Teams in der Vorgründungsphase unterstützen."@de ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:founding_status ;
        sh:property [
            sh:path ff:founding_status ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;
    ] .

ff:composition_founding_team a ff:DataField ;
    schema:category ff:gruendung-df-kategorie ;
    rdfs:label "Startup Team Composition"@en, "Zusammensetzung des Gründungsteams"@de ;
    schema:question "What is the educational and professional background of your startup team members?"@en, 
                    "Welchen Bildungs- und Berufsabschluss haben die Mitglieder Ihres Gründungsteams?"@de ;
    rdfs:comment "Some funding programs take the educational composition of founding teams into account, especially regarding the presence of academic versus non-academic backgrounds. This helps determine eligibility for grants aimed at research-based startups or those supporting founders without formal academic degrees."@en,
                 "Einige Förderprogramme berücksichtigen die Bildungszusammensetzung von Gründungsteams, insbesondere in Bezug auf akademische und nicht-akademische Hintergründe. Dies kann für Förderungen relevant sein, die sich gezielt an forschungsbasierte Startups oder Gründer ohne akademischen Abschluss richten."@de ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:composition_founding_team ;
        sh:property [
            sh:path ff:composition_founding_team ;
            sh:in (
                ff:composition_founding_team-only-academic-degrees
                ff:composition_founding_team-at-most-one-non-academic-degree
                ff:composition_founding_team-more-than-one-non-academic-degree
                ff:composition_founding_team-at-least-one-non-degree
            ) ;
            sh:maxCount 1 ;
        ] ;
    ] .
ff:composition_founding_team-only-academic-degrees a ff:AnswerOption ;
    rdfs:label "Only team members with academic degrees (e.g., bachelor's, master's, PhD)"@en, 
               "Nur Teammitglieder mit akademischen Abschlüssen (z. B. Bachelor, Master, Promotion)"@de .
ff:composition_founding_team-at-most-one-non-academic-degree a ff:AnswerOption ;
    rdfs:label "Mostly academic degrees, but at most one team member has a non-academic professional qualification"@en, 
               "Überwiegend akademische Abschlüsse, aber maximal ein Teammitglied mit nicht-akademischem Berufsabschluss"@de .
ff:composition_founding_team-more-than-one-non-academic-degree a ff:AnswerOption ;
    rdfs:label "More than one team member has a non-academic professional qualification (e.g., vocational training)"@en, 
               "Mehr als ein Teammitglied hat einen nicht-akademischen Berufsabschluss (z. B. Berufsausbildung)"@de .
ff:composition_founding_team-at-least-one-non-degree a ff:AnswerOption ;
    rdfs:label "At least one team member has no formal professional or academic qualification"@en, 
               "Mindestens ein Teammitglied hat keinen formalen Berufs- oder akademischen Abschluss"@de .

ff:secondary_activity a ff:DataField ;
    schema:category ff:employment-df-kategorie ;
    rdfs:label "Paid Secondary Activities"@en, "Bezahlte Nebentätigkeiten"@de ;
    schema:question "How many hours per week do you spend on paid secondary activities alongside your main work or startup?"@en, 
                    "Wie viele Stunden pro Woche verbringen Sie mit bezahlten Nebentätigkeiten neben Ihrer Haupttätigkeit oder Ihrem Startup?"@de ;
    rdfs:comment "Some funding programs consider the extent of paid secondary activities when assessing eligibility. Part-time employment or freelance work beyond a certain threshold may affect funding opportunities or eligibility for certain grants."@en,
                 "Einige Förderprogramme berücksichtigen den Umfang bezahlter Nebentätigkeiten bei der Förderfähigkeit. Eine Teilzeitbeschäftigung oder freiberufliche Tätigkeit über eine bestimmte Grenze hinaus kann Auswirkungen auf Fördermöglichkeiten oder die Förderfähigkeit haben."@de ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:secondary_activity ;
        sh:property [
            sh:path ff:secondary_activity ;
            sh:in (
                ff:secondary_activity-no-secondary
                ff:secondary_activity-less-than-20
                ff:secondary_activity-more-than-20
            ) ;
            sh:maxCount 1 ;
        ] ;
    ] .
ff:secondary_activity-no-secondary a ff:AnswerOption ;
    rdfs:label "I do not have any paid secondary activities"@en, 
               "Ich habe keine bezahlten Nebentätigkeiten"@de .
ff:secondary_activity-less-than-20 a ff:AnswerOption ;
    rdfs:label "Less than 20 hours per week"@en, 
               "Weniger als 20 Stunden pro Woche"@de .
ff:secondary_activity-more-than-20 a ff:AnswerOption ;
    rdfs:label "More than 20 hours per week"@en, 
               "Mehr als 20 Stunden pro Woche"@de .
    
ff:studies_status a ff:DataField ;
    schema:category ff:education-df-kategorie ;
    rdfs:label "Study Progress"@en, "Studienfortschritt"@de ;
    schema:question "What is your current progress in your academic studies?"@en, 
                    "Wie weit sind Sie in Ihrem akademischen Studium fortgeschritten?"@de ;
    rdfs:comment "Some funding programs take into account the stage of your academic studies when determining eligibility. Certain grants may be available only to students who have completed a minimum portion of their studies or to those who have already graduated."@en,
                 "Einige Förderprogramme berücksichtigen den Fortschritt Ihres akademischen Studiums bei der Feststellung der Förderfähigkeit. Bestimmte Förderungen sind möglicherweise nur für Studierende verfügbar, die einen bestimmten Teil ihres Studiums abgeschlossen haben, oder für Absolventen."@de ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:studies_status ;
        sh:property [
            sh:path ff:studies_status ;
            sh:in (
                ff:studies_status-less-than-half
                ff:studies_status-more-than-half
                ff:studies_status-completed
                ff:studies_status-no-academic-studies
            ) ;
            sh:maxCount 1 ;
        ] ;
    ] .
ff:studies_status-less-than-half a ff:AnswerOption ;
    rdfs:label "I have completed less than half of my academic studies"@en, 
               "Ich habe weniger als die Hälfte meines Studiums abgeschlossen"@de .
ff:studies_status-more-than-half a ff:AnswerOption ;
    rdfs:label "I have completed more than half of my academic studies"@en, 
               "Ich habe mehr als die Hälfte meines Studiums abgeschlossen"@de .
ff:studies_status-completed a ff:AnswerOption ;
    rdfs:label "I have completed my academic studies (graduated)"@en, 
               "Ich habe mein Studium abgeschlossen (absolviert)"@de .
ff:studies_status-no-academic-studies a ff:AnswerOption ;
    rdfs:label "I have never been enrolled in an academic study program"@en, 
               "Ich war nie in einem akademischen Studiengang eingeschrieben"@de .

ff:academic_institution_support a ff:DataField ;
    schema:category ff:gruendung-df-kategorie ;
    rdfs:label "Support for Founding Teams from Academic Institutions"@en, "Unterstützung für Gründungsteams durch akademische Einrichtungen"@de ;
    schema:question "Does your academic institution offer dedicated support for startup founders, such as mentorship, incubator programs, or funding opportunities?"@en, 
                    "Bietet Ihre akademische Einrichtung gezielte Unterstützung für Gründer*innen, wie Mentoring, Inkubator-Programme oder Finanzierungsmöglichkeiten?"@de ;
    rdfs:comment "Some universities and research institutions provide dedicated startup support, such as startup centers, incubators, or funding opportunities for early-stage ventures. This can be relevant for grants that require institutional backing or offer additional resources to university-affiliated teams."@en,
                 "Einige Universitäten und Forschungseinrichtungen bieten gezielte Unterstützung für Gründer*innen an, wie Gründerzentren, Inkubator-Programme oder finanzielle Förderungen für junge Unternehmen. Dies kann für Förderprogramme relevant sein, die eine institutionelle Unterstützung voraussetzen oder zusätzliche Ressourcen für universitätsnahe Teams bereitstellen."@de ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:academic_institution_support ;
        sh:property [
            sh:path ff:academic_institution_support ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ]
    ] .

##############################
# Updated fields for IBB Gründungsbonus
##############################

ff:bank_account_germany a ff:DataField ;
    schema:category ff:gruendung-df-kategorie ;
    rdfs:label "Bank Account in Germany"@en, "Bankkonto in Deutschland"@de ;
    schema:question "Does your startup have a bank account in Germany?"@en, 
                    "Hat Ihr Startup ein Bankkonto in Deutschland?"@de ;
    rdfs:comment "Some funding programs require startups to have a bank account in Germany to receive financial support or grants. This question helps determine eligibility for grants that are specifically available to companies with a German bank account."@en,
                    "Einige Förderprogramme setzen voraus, dass Startups ein Bankkonto in Deutschland haben, um finanzielle Unterstützung oder Fördermittel zu erhalten. Diese Frage hilft dabei, die Förderfähigkeit für Förderungen zu bestimmen, die speziell für Unternehmen mit einem deutschen Bankkonto verfügbar sind."@de ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:bank_account_germany ;
        sh:property [
            sh:path ff:bank_account_germany ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;
    ] .

ff:company_age a ff:DataField ;
    schema:category ff:gruendung-df-kategorie ;
    rdfs:label "Company Age"@en, "Alter des Unternehmens"@de ;
    schema:question "When was your startup founded?"@en, 
                    "Wann wurde Ihr Unternehmen gegründet?"@de ;
    rdfs:comment "The age of your startup can be relevant for funding opportunities that target early-stage ventures or require companies to be founded within a specific timeframe. Some grants are specifically available for startups founded less than 18 months ago or for companies that are not yet established."@en,
                    "Das Alter Ihres Unternehmens kann für Fördermöglichkeiten relevant sein, die sich an junge Unternehmen richten oder voraussetzen, dass Unternehmen innerhalb eines bestimmten Zeitraums gegründet wurden. Einige Förderungen sind speziell für Startups verfügbar, die vor weniger als 18 Monaten gegründet wurden oder für Unternehmen, die noch nicht etabliert sind."@de ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:company_age ;
        sh:property [
            sh:path ff:company_age ;
            sh:in (
                ff:company_age-less-than-12-months
                ff:company_age-less-than-18-months
                ff:company_age-less-than-5-years
                ff:company_age-more-than-5-years
                ff:company_age-not-yet-founded
            ) ;
            sh:maxCount 1 ;
        ] ;
    ] .
ff:company_age-less-than-12-months a ff:AnswerOption ; rdfs:label "Less than 12 months ago"@en, "Vor weniger als 12 Monaten"@de .
ff:company_age-less-than-18-months a ff:AnswerOption ; rdfs:label "Less than 18 months ago"@en, "Vor weniger als 18 Monaten"@de .
ff:company_age-more-than-18-months a ff:AnswerOption ; rdfs:label "More than 18 months ago"@en, "Vor mehr als 18 Monaten"@de .
ff:company_age-less-than-5-years a ff:AnswerOption ; rdfs:label "Less than 5 years ago"@en, "Vor weniger als 5 Jahren"@de .
ff:company_age-more-than-5-years a ff:AnswerOption ; rdfs:label "More than 5 years ago"@en, "Vor mehr als 5 Jahren"@de .
ff:company_age-not-yet-founded a ff:AnswerOption ; rdfs:label "Not yet founded"@en, "Noch nicht gegründet"@de .
            
ff:headquarters_location a ff:DataField ;
    schema:category ff:gruendung-df-kategorie ;
    rdfs:label "Headquarters Location"@en, "Hauptsitz des Unternehmens"@de ;
    schema:question "Where is your company headquartered?"@en, "Wo befindet sich der Hauptsitz Ihres Unternehmens?"@de ;

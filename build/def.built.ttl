          sh:datatype xsd:boolean;
          sh:maxCount 1;
          sh:path ff:taxableGermany
        ];
      sh:targetSubjectsOf ff:taxableGermany
    ].

ff:unemploymentBenefitApproved a ff:DataField;
  schema:category ff:financeDfKategorie;
  schema:question
    "Wurde Ihnen in der Vergangenheit Arbeitslosengeld bewilligt?"@de-x-es,
    "Wurde Ihnen in der Vergangenheit Arbeitslosengeld bewilligt?"@de,

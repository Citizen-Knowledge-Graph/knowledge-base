ff:workplaceType a ff:DataField;
  schema:category ff:beschaeftigung-df-kategorie;
  schema:question
    "Welche Art von Arbeitsstätte haben Sie?"@de-x-es,
    "Welche Art von Arbeitsstätte haben Sie?"@de,
    "What type of workplace do you have?"@en;
  rdfs:comment
    "Bitte geben Sie an, ob Ihre Arbeitsstätte ein Büro, eine Fabrik, ein Geschäft, eine Schule, ein Krankenhaus oder eine andere Art von Einrichtung ist."@de-x-es,
    "Bitte geben Sie an, ob Ihre Arbeitsstätte ein Büro, eine Fabrik, ein Geschäft, eine Schule, ein Krankenhaus oder eine andere Art von Einrichtung ist."@de,
    "Please indicate whether your workplace is an office, factory, store, school, hospital, or another type of facility."@en;
  rdfs:label
    "Art der Arbeitsstätte"@de-x-es,
    "Art der Arbeitsstätte"@de,
    "Type of Workplace"@en;
  ff:hasShaclShape [ a sh:NodeShape;
      sh:property [
          sh:in (ff:workplaceType-regular ff:workplaceType-inclusive ff:workplaceType-sheltered ff:workplaceType-selfEmployed ff:workplaceType-other);
          sh:maxCount 1;
          sh:path ff:workplaceType
        ];
      sh:targetSubjectsOf ff:workplaceType
    ].

ff:workplaceType-inclusive a ff:AnswerOption;
  rdfs:label
    "Inklusive Arbeitsstätte"@de-x-es,
    "Inklusive Arbeitsstätte"@de,
    "Inclusive Workplace"@en.

ff:workplaceType-other a ff:AnswerOption;
  rdfs:label
    "Andere"@de-x-es,
    "Andere"@de,
    "Other"@en.

ff:workplaceType-regular a ff:AnswerOption;
  rdfs:label
    "Reguläre Arbeitsstätte"@de-x-es,
    "Reguläre Arbeitsstätte"@de,
    "Regular Workplace"@en.

ff:workplaceType-selfEmployed a ff:AnswerOption;
  rdfs:label
    "Selbstständig"@de-x-es,
    "Selbstständig"@de,
    "Self-employed"@en.

ff:workplaceType-sheltered a ff:AnswerOption;
  rdfs:label
    "Geschützte Werkstatt"@de-x-es,
    "Geschützte Werkstatt"@de,
    "Sheltered Workshop"@en.


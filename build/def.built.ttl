ff:CalculateAgeFromBirthdate a ff:MaterializationRule;
  rdfs:label
    "Berechnung des Alters basierend auf dem Geburtsdatum"@de-x-es,
    "Berechnung des Alters basierend auf dem Geburtsdatum"@de,
    "Calculate the age from the birthdate"@en;
  ff:input ff:geburtsdatum;
  ff:output ff:hasAge;
  ff:sparqlConstructQuery "\n        PREFIX ff: <https://foerderfunke.org/default#>\n        CONSTRUCT {\n            ?person ff:hasAge ?age .\n        } WHERE {\n            ?person ff:geburtsdatum ?bday .\n            BIND(YEAR(NOW()) - YEAR(?bday) - IF(MONTH(NOW()) < MONTH(?bday) || (MONTH(NOW()) = MONTH(?bday) && DAY(NOW()) < DAY(?bday)), 1, 0) AS ?age) .\n        }\n    ".


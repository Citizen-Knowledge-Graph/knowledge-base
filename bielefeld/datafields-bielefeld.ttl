            sh:maxCount 1 ;
        ] ;
    ] .


ff:job_at_risk_without_equal_status a ff:DataField ;
    schema:category ff:arbeitswelt-df-kategorie ;
    rdfs:label "Arbeitsplatz gefährdet ohne Gleichstellung"@de , "Job at risk without equal status"@en ;
    schema:question "Könnten Sie wegen Ihrer Behinderung ohne eine Gleichstellung einen geeigneten Arbeitsplatz nicht erlangen oder behalten?"@de ,
        "Would you not be able to obtain or retain suitable employment due to your disability without equal status?"@en ;
    rdfs:comment "Gleichstellung bedeutet, dass Personen mit einem Grad der Behinderung von 30 bis 49 formal Schwerbehinderten gleichgestellt werden, wenn sie sonst keinen geeigneten Arbeitsplatz erlangen oder behalten könnten."@de ,
        "Equal status means that persons with a degree of disability of 30 to 49 are formally treated as severely disabled if they would otherwise not be able to obtain or retain suitable employment."@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:job_at_risk_without_equal_status ;
        sh:property [
            sh:path ff:job_at_risk_without_equal_status ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;
    ] .

ff:open_market_fit a ff:DataField ;
    schema:category ff:arbeitswelt-df-kategorie ;
    rdfs:label "Geeignet für den allgemeinen Arbeitsmarkt"@de , "Fit for the open labor market"@en ;
    schema:question "Sind Sie außerhalb einer geschützten Einrichtung in der Lage, auf dem regulären Arbeitsmarkt zu arbeiten?"@de ,
        "Are you able to work on the regular labor market outside a sheltered facility?"@en ;
    rdfs:comment "Der allgemeine Arbeitsmarkt sind alle normalen Arbeitsplätze in Firmen, an denen Menschen – auch mit Hilfsmitteln oder Unterstützung – arbeiten können, solange es kein dauerhaft geschützter Sonder­arbeitsplatz wie eine Werkstatt für behinderte Menschen ist."@de ,
        "The open labor market includes all regular jobs in companies where people can work, even with aids or support, as long as it is not a permanently protected special workplace like a workshop for disabled people."@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:open_market_fit ;
        sh:property [
            sh:path ff:open_market_fit ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;
    ] .

ff:open_market_hours a ff:DataField ;
    schema:category ff:arbeitswelt-df-kategorie ;
    rdfs:label "Stunden pro Tag auf dem allgemeinen Arbeitsmarkt"@de , "Hours per day on the open labor market"@en ;
    schema:question "Wie viele Stunden könnten Sie unter den üblichen Bedingungen des allgemeinen Arbeitsmarktes pro Tag arbeiten?"@de ,
        "How many hours could you work per day under the usual conditions of the open labor market?"@en ;
    rdfs:comment "Diese Frage klärt, wie viele Stunden Sie unter den üblichen Bedingungen des allgemeinen Arbeitsmarktes arbeiten können; die Einstufung beruht auf ärztlichen Gutachten und einer sozialmedizinischen Bewertung Ihres verbliebenen Leistungsvermögens."@de ,
        "This question clarifies how many hours you can work per day under the usual conditions of the open labor market; the classification is based on medical reports and a social medical assessment of your remaining capacity."@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:open_market_hours ;
        sh:property [
            sh:path ff:open_market_hours ;
            sh:in (ff:open_market_hours-3hours ff:open_market_hours-6hours ff:open_market_hours-more_than_6hours) ;
            sh:maxCount 1 ;
        ] ;
    ] .

ff:open_market_hours-3hours a ff:AnswerOption ; rdfs:label "Höchstens 3 Stunden"@de , "At most 3 hours"@en .
ff:open_market_hours-6hours a ff:AnswerOption ; rdfs:label "Höchstens 6 Stunden"@de , "At most 6 hours"@en .
ff:open_market_hours-more_than_6hours a ff:AnswerOption ; rdfs:label "Mehr als 6 Stunden"@de , "More than 6 hours"@en .

ff:special_cause_inability a ff:DataField ;
    schema:category ff:arbeitswelt-df-kategorie ;
    rdfs:label "Besondere Gründe für Arbeitsunfähigkeit"@de , "Special reasons for inability to work"@en ;
    schema:question "Ist Ihre Erwerbsminderung durch einen Arbeitsunfall, eine Berufskrankheit, einen Unfall im Wehr- oder Freiwilligendienst, einen Impfschaden oder durch Gewalt entstanden?"@de ,
        "Is your disability due to a work accident, an occupational disease, an accident during military or voluntary service, a vaccination injury, or violence?"@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:special_cause_inability ;
        sh:property [
            sh:path ff:special_cause_inability ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;
    ] .

ff:general_waiting_period a ff:DataField ;
    schema:category ff:arbeitswelt-df-kategorie ;
    rdfs:label "Allgemeine Wartezeit"@de , "Regular waiting period"@en ;
    schema:question "Haben Sie insgesamt mindestens fünf Jahre Zeiten, in denen Beiträge zur gesetzlichen Rentenversicherung gezahlt wurden oder angerechnete Monate zählen?"@de , 
        "Do you have a total of at least five years of periods during which contributions to the statutory pension insurance were paid or credited months count?"@en ;
    rdfs:comment "Die allgemeine Wartezeit gilt als erfüllt, wenn sich mindestens 60 Kalendermonate mit Beiträgen oder mit gleichgestellten Monaten angesammelt haben; gleichgestellte Monate sind Zeiträume ohne Beiträge, die trotzdem zählen, etwa während Kindererziehung, Schul- oder Studienbesuch, Krankheit, Reha oder Pflege eines Angehörigen."@de ,
        "The regular waiting period is considered fulfilled if at least 60 calendar months with contributions or credited months have accumulated; credited months are periods without contributions that still count, such as during child-rearing, school or university attendance, illness, rehabilitation, or caregiving for a relative."@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:general_waiting_period ;
        sh:property [
            sh:path ff:general_waiting_period ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;
    ] .

ff:extended_waiting_period a ff:DataField ;
    schema:category ff:arbeitswelt-df-kategorie ;
    rdfs:label "Erweiterte Wartezeit"@de , "Extended waiting period"@en ;
    schema:question "Haben Sie, seit Sie ununterbrochen voll erwerbsgemindert sind, insgesamt mindestens 20 Jahre Zeiten in der gesetzlichen Rentenversicherung angesammelt?"@de ,
        "Since you have been continuously fully incapable of work, have you accumulated at least 20 years of credited periods in the German statutory pension insurance?"@en ;
    rdfs:comment "Diese besondere Wartezeit gilt für Personen, die schon vor den ersten fünf Beitragsjahren voll erwerbsgemindert waren und es bis heute ohne Unterbrechung geblieben sind. In diesem Fall zählen 240 Kalendermonate rentenrechtlicher Zeiten – meist Anrechnungs- oder Berücksichtigungszeiten wie Schul-, Reha-, Krankheits- oder Pflegemonate –, auch wenn während dieser Zeit kaum oder keine Beiträge gezahlt werden konnten."@de ,
        "This special waiting period applies to individuals who were already fully incapable of work before the first five contribution years and have remained so continuously until today. In this case, 240 calendar months of pension-relevant periods count – usually credited or recognized periods such as school, rehabilitation, illness, or caregiving months – even if few or no contributions could be paid during this time."@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:extended_waiting_period ;
        sh:property [
            sh:path ff:extended_waiting_period ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;
    ] .

ff:compulsory_contributions a ff:DataField ;
    schema:category ff:arbeitswelt-df-kategorie ;
    rdfs:label "Pflichtbeiträge zur gesetzlichen Rentenversicherung"@de , "Compulsory contributions to statutory pension insurance"@en ;
    schema:question "Haben Sie in den letzten 5 Jahren mindestens 36 Monate Pflichtbeiträge zur gesetzlichen Rentenversicherung gezahlt?"@de ,
        "Have you paid at least 36 months of compulsory contributions to the statutory pension insurance in the last 5 years?"@en ;
    rdfs:comment "Pflichtbeiträge sind Beträge, die aus einer versicherten Beschäftigung oder einer Pflichtversicherung automatisch abgeführt werden. Der Prüfzeitraum umfasst normalerweise die letzten fünf Jahre vor Eintritt der Erwerbsminderung; er verlängert sich jedoch um beitragsfreie Monate wie Krankheits- oder Reha­zeiten, Zeiten einer bereits gezahlten Erwerbsminderungsrente, Kinder- oder Pflegezeiten, bestimmte unbezahlte Auszeiten sowie bis zu sieben Jahre Schul- oder Studienzeit nach dem 17. Geburtstag. Dadurch können auch weiter zurückliegende Pflichtbeiträge berücksichtigt werden."@de ,
        "Compulsory contributions are amounts automatically deducted from an insured employment or compulsory insurance. The review period usually covers the last five years before the onset of disability; however, it extends to include contribution-free months such as illness or rehabilitation periods, periods of already paid disability pensions, child-rearing or caregiving periods, certain unpaid breaks, and up to seven years of school or university time after the age of 17. This allows for consideration of compulsory contributions made further back in time."@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:compulsory_contributions ;
        sh:property [
            sh:path ff:compulsory_contributions ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;
    ] .

ff:has_confirmed_addiction a ff:DataField ;
    schema:category ff:gesundheit-pflege-df-kategorie ;
    rdfs:label "Bestätigte Sucht"@de , "Confirmed addiction"@en ;
    schema:question "Leiden Sie an einer Sucht, die ärztlich bestätigt wurde?"@de ,
        "Do you have an addiction that has been confirmed by a doctor or psychologist?"@en ;
    rdfs:comment "Eine Sucht wird von einer Ärzt:in bestätigt, wenn mehrere eindeutige Anzeichen vorliegen – zum Beispiel, dass der Konsum nicht mehr kontrolliert werden kann, der Körper ohne die Substanz starke Beschwerden zeigt, ein sehr starkes Verlangen danach besteht und andere wichtige Dinge im Leben vernachlässigt werden."@de ,
        "An addiction is confirmed by a doctor or psychologist when there are multiple clear signs – for example, that consumption can no longer be controlled, the body shows severe discomfort without the substance, there is a very strong craving for it, and other important aspects of life are neglected."@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:has_confirmed_addiction ;
        sh:property [
            sh:path ff:has_confirmed_addiction ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;
    ] .

ff:has_medical_insurance a ff:DataField ;
    schema:category ff:gesundheit-pflege-df-kategorie ;
    rdfs:label "Krankenversicherung"@de , "Health insurance"@en ;
    schema:question "Haben Sie eine Krankenversicherung?"@de , "Do you have health insurance?"@en ;
    rdfs:comment "Eine Krankenversicherung ist notwendig, um medizinische Behandlungen und Therapien zu erhalten. Sie kann gesetzlich oder privat sein."@de ,
        "Health insurance is necessary to receive medical treatments and therapies. It can be statutory or private."@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:has_medical_insurance ;
        sh:property [
            sh:path ff:has_medical_insurance ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;
    ] .

ff:sobriety_wish a ff:DataField ;
    schema:category ff:gesundheit-pflege-df-kategorie ;
    rdfs:label "Abstinenz Wunsch"@de , "Sobriety wish"@en ;
    schema:question "Wünschen Sie, von Ihrer Sucht loszukommen?"@de , "Do you wish to overcome your addiction?"@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;

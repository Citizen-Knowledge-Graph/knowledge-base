    ] .

ff:has_severe_disease a ff:DataField ;
    schema:category ff:gesundheit-pflege-df-kategorie ;
    rdfs:label "Schwere Erkrankung"@de , "Severe disease"@en ;
    schema:question "Haben Sie eine schwere Krankheit, zum Beispiel Krebs, HIV, Hepatitis A oder Hepatitis C??"@de , 
        "Do you have a severe disease, such as cancer, HIV, hepatitis A, or hepatitis C?"@en ;
    rdfs:comment "Eine schwere Erkrankung ist eine Krankheit, die erhebliche Auswirkungen auf Ihre Gesundheit und Lebensqualität hat."@de ,
        "A severe disease is an illness that has significant effects on your health and quality of life."@en ;
    ff:hasShaclShape [

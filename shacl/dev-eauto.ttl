@prefix ff: <https://foerderfunke.org/default#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

ff:metadata ff:id "eauto" ;
    ff:title "E-Auto Zuschuss" ;
    ff:category "WÃ¤rmewende" .

ff:CitizenOwnsElectricCarShape
    a sh:NodeShape ;
    sh:targetClass ff:Citizen ;
    sh:property [
                    sh:path ff:owns ;
                    sh:qualifiedValueShape ff:ElectricCarShape ;
                    sh:qualifiedMinCount 1 ;
                ] ;
    sh:property [
                    sh:path ff:hasResidence ;
                    sh:in ("Berlin") ;
                ] ;
    sh:property [
                    sh:path ff:hasDrivingLicense ;
                    sh:in (true) ;
                ] .

ff:ElectricCarShape
    a sh:NodeShape ;
    sh:targetClass ff:Car ;
    sh:property [
                    sh:path rdf:type ;
                    sh:hasValue ff:Car ;
                ] ;
    sh:property [
                    sh:path ff:carType ;
                    sh:hasValue "Electric" ;
                ] ;
    sh:property [
                    sh:path ff:carAge ;
                    sh:maxInclusive 5 ;
                    sh:datatype xsd:integer ;
                ] .

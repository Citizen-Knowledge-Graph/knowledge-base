@prefix ff: <https://foerderfunke.org/default#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

ff:daemmungRP a ff:RequirementProfile ;
    ff:title "DÃ¤mmungszuschuss" ;
    ff:category ff:Waermewende .

ff:MainPersonShape a sh:NodeShape ;
    sh:targetClass ff:Citizen ;
    sh:property [
        sh:path ff:hasIncomeBrutto ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
    ] .

ff:CitizenOwnsHouseShape
    a sh:NodeShape ;
    sh:targetClass ff:Citizen ;
    sh:property [
          sh:path ff:owns ;
          sh:class ff:House ;
          sh:minCount 1 ;
      ] .

ff:HouseEligibleForInsulationBonusShape
    a sh:NodeShape ;
    sh:targetClass ff:House ;
    sh:property [
          sh:path ff:roofArea ;
          sh:minCount 1 ;
          sh:datatype xsd:integer ;
          sh:minInclusive 50 ;
          sh:message "50" ;
      ] .

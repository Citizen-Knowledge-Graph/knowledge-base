ff:no_double_funding a ff:DataField, ff:SozialleistungsfinderDataField ;
    schema:category ff:gruendung-df-kategorie ;
    rdfs:label "No Double Funding"@en, "Keine Doppelförderung"@de ;
    schema:question "Are you receiving or planning to receive other public funding for the same eligible expenses?"@en, "Erhalten Sie oder planen Sie andere öffentliche Fördermittel für die gleichen förderfähigen Kosten?"@de ;

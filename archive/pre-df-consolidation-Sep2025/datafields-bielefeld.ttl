    ] .

ff:opioid_addiction_duration_2years a ff:DataField ;
    schema:category ff:gesundheit-pflege-df-kategorie ;
    rdfs:label "Sucht seit mindestens 2 Jahren"@de , "Addiction for at least 2 years"@en ;
    schema:question "Besteht Ihre Opioidabhängigkeit seit mehr als 2 Jahren?"@de , "Sind sie schon länger als 2 Jahre abhängig von Opioiden?"@de-x-es, "Have you been dependent on opioids for more than 2 years?"@en ;
    rdfs:comment "Eine Therapie mit dem Wirkstoff Diamorphin kann nur genehmigt werden, wenn die Abhängigkeit seit mehr als 2 Jahren besteht."@de, "Eine Therapie mit Diamorphin kann nur genehmigt werden, wenn die Abhängigkeit schon länger als 2 Jahre besteht."@de-x-es, "Treatment with the active ingredient diamorphine can only be approved if the addiction has existed for more than two years."@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:opioid_addiction_duration_2years ;
        sh:property [
            sh:path ff:opioid_addiction_duration_2years ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;        
    ] .

ff:addiction_damage a ff:DataField ;
    schema:category ff:gesundheit-pflege-df-kategorie ;
    rdfs:label "Suchtschäden"@de, "Addiction damage"@en ;
    schema:question "Weisen Sie erhebliche Defizite in medizinischer, psychologischer oder sozialer Hinsicht auf, die auf den Gebrauch von unerlaubt erworbenen Opioiden zurückzuführen sind?"@de , "Haben Sie große Probleme mit Ihrer Gesundheit, Ihrer Psyche oder in Ihrem sozialen Umfeld? Haben Sie diese Probleme, weil Sie illegale Opioide zu sich genommen haben?"@de-x-es , "Do you have significant medical, psychological, or social deficits attributable to the use of illicitly obtained opioids?"@en ;
    rdfs:comment "Solche Probleme können zum Beispiel schlechte körperliche Gesundheit, Traurigkeit oder Angst, Streit mit Familie oder Freund:innen, Verlust der Arbeit oder Schwierigkeiten mit Wohnung oder Geld sein."@de , "Such problems can include poor physical health, feeling depressed or anxious, trouble with family or friends, losing your job, or difficulties with housing or money."@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:addiction_damage ;
        sh:property [
            sh:path ff:addiction_damage ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;        
    ] .

ff:substitutiontherapy_regular_6months a ff:DataField ;
    schema:category ff:gesundheit-pflege-df-kategorie ;
    rdfs:label "Reguläre Substitutionstherapie von mindestens 6 Monaten"@de , "Regular substitution therapy of at least 6 months"@en ;
    schema:question "Waren Sie bereits in Ihrem Leben mindestens 6 Monate in einer Substitutionstherapie?"@de ,
        "Waren Sie schon einmal mindestens 6 Monate in einer Substitutionstherapie?"@de-x-es ,
        "Have you already been in substitution therapy for at least 6 months in your life?"@en ;
    rdfs:comment "Eine Substitutionstherapie ist eine medizinische Behandlung, bei der Opioide (zum Beispiel Heroin) durch andere, länger wirkende und weniger schädliche Substanzen (zum Beispiel Methadon oder Buprenorphin) ersetzt werden."@de ,
        "Eine Substitutionstherapie ist eine Behandlung. Dabei wird ein Rauschmittel wie Heroin durch ein anderes Medikament ersetzt. Zum Beispiel Methadon oder Buprenorphin. Diese Medikamente wirken länger und sind weniger schädlich."@de-x-es ,
        "Substitution therapy is a medical treatment in which opioids (for example, heroin) are replaced by other, longer-acting and less harmful substances (for example, methadone or buprenorphine)."@en ;        
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:substitutiontherapy_regular_6months ;
        sh:property [
            sh:path ff:substitutiontherapy_regular_6months ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;        
    ] .

ff:substitutiontherapy_regular_unsuccessful a ff:DataField ;
    schema:category ff:gesundheit-pflege-df-kategorie ;
    rdfs:label "Erfolglose Substitutionstherapie"@de, "Unsuccessful substitution therapy"@en ;
    schema:question "Können Sie nachweisen, dass Sie bereits eine Substitutionstherapie zur Behandlung einer Opioidabhängigkeit über einen Zeitraum von 6 Monaten gemacht haben, welche sich als nicht geeignet oder erfolglos erwiesen hat?"@de , 
        "Können Sie belegen, dass Sie schon eine Substitutionstherapie gegen eine Abhängigkeit von Opioiden gemacht haben, die mindestens 6 Monate gedauert hat und nicht geholfen hat?"@de-x-es , 
        "Can you prove that you have already undergone substitution therapy for the treatment of opioid dependence for a period of 6 months, which proved to be unsuitable or unsuccessful?"@en ;
    rdfs:comment "Bei einer Substitutionstherapie zur Behandlung einer Opioidabhängigkeit bekommen Betroffene ein Ersatzmedikament wie Methadon, Polamidon oder Buprenorphin. Wenn diese Behandlung mindestens 6 Monate lang durchgeführt wurde und sich als ungeeignet oder erfolglos erwiesen hat, gilt sie als nicht wirksam."@de , 
        "In einer Substitutionstherapie gegen eine Abhängigkeit von Opioiden bekommen Menschen ein Ersatzmedikament. Beispiele sind Methadon, Polamidon oder Buprenorphin. Hat die Behandlung mindestens 6 Monate gedauert und nicht geholfen, gilt sie als erfolglos."@de-x-es , 
        "In substitution therapy for the treatment of opioid dependence, patients are given a substitute medication such as methadone, polamidone, or buprenorphine. If this treatment has been carried out for at least 6 months and has proven to be unsuitable or unsuccessful, it is considered ineffective."@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:substitutiontherapy_regular_unsuccessful ;
        sh:property [
            sh:path ff:substitutiontherapy_regular_unsuccessful ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;        
    ] .
        
ff:requires_assistance a ff:DataField ;
    schema:category ff:gesundheit-pflege-df-kategorie ;
    rdfs:label "Erheblicher Unterstützungsbedarf"@de , "Requires assistance"@en ;
    schema:question "Benötigen Sie regelmäßige, erhebliche und dauerhafte Unterstützung, um Ihre Arbeit auszuführen?"@de , 
        "Brauchen Sie oft viel und immer wieder Unterstützung?"@de-x-es , 
        "Do you require regular, substantial, and ongoing support to perform your work?"@en ;
    rdfs:comment "Das bedeutet, dass Sie über eine lange Zeit hinweg oft und in größerem Umfang Hilfe von anderen brauchen, um wichtige Dinge im Alltag oder bei der Arbeit erledigen zu können."@de ,
        "Das heißt: Sie brauchen über eine längere Zeit oft und deutlich Hilfe von anderen. Nur so können Sie wichtige Dinge im Alltag oder bei der Arbeit schaffen."@de-x-es ,
        "This means that you need frequent and significant help from others over a long period to manage important tasks in daily life or at work."@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:requires_assistance ;
        sh:property [
            sh:path ff:requires_assistance ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;        
    ] .

ff:core_activity_independent a ff:DataField ;
    schema:category ff:gesundheit-pflege-df-kategorie ;
    rdfs:label "Kernaktivität selbstständig"@de , "Core activity independent"@en ;
    schema:question "Können Sie die Kerntätigkeit Ihrer Arbeit ohne Hilfe erledigen?"@de , 
        "Können Sie die wichtigste Aufgabe Ihrer Arbeit alleine machen?"@de-x-es,
         "Can you perform the core tasks of your job without assistance?"@en ;
    rdfs:comment "Sie erledigen die wichtigste Aufgabe selbst und brauchen nur bei kleineren oder vorbereitenden Arbeiten Unterstützung."@de ,
        "Das heißt: Sie machen die Hauptarbeit selbst. Nur bei kleineren oder vorbereitenden Arbeiten brauchen Sie Hilfe."@de-x-es ,
        "You can perform the main task independently and only need support for smaller or preparatory tasks."@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:core_activity_independent ;
        sh:property [
            sh:path ff:core_activity_independent ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;        
    ] .

ff:existing_workplace_measures_sufficient a ff:DataField ;
    schema:category ff:gesundheit-pflege-df-kategorie ;
    rdfs:label "Vorhandene Arbeitsplatzgestaltung unzureichend"@de , "Existing workplace design insufficient"@en ;
    schema:question "Ist die vorhandene behindertengerechte Gestaltung Ihres Arbeitsplatzes ausreichend, um Ihre Arbeit zu ermöglichen?"@de ,
        "Reicht die behindertengerechte Ausstattung an Ihrem Arbeitsplatz aus, damit Sie arbeiten können?"@de-x-es ,
        "Is the existing disability-friendly design of your workplace sufficient to enable you to work?"@en ;
    rdfs:comment "Bestehende Anpassungen können zum Beispiel ein höhenverstellbarer Schreibtisch, eine Rampe oder ein Bildschirmlesegerät sein."@de ,
        "Beispiele sind: ein Schreibtisch, den man hoch und runter stellen kann, eine Rampe oder ein Gerät zum Vorlesen am Bildschirm."@de-x-es ,
        "Existing adjustments can include a height-adjustable desk, a ramp, or a screen reader."@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:existing_workplace_measures_sufficient ;
        sh:property [
            sh:path ff:existing_workplace_measures_sufficient ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;        
    ] .


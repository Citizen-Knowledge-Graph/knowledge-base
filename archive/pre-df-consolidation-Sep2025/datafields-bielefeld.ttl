ff:registered_residence_country a ff:DataField ;
    schema:category ff:persoenliche-angaben-df-kategorie ;
    rdfs:label "Land der Meldeadresse"@de , "Country of registered residence"@en ;
    schema:question "In welchem Land sind Sie mit einer Meldeadresse gemeldet?"@de , 
        "In welchem Land ist Ihre Wohnadresse gemeldet?"@de-x-es ,
        "In which country is your residence officially registered?"@en ;
    rdfs:comment "Es geht um das Land, in dem Sie eine offizielle Meldeadresse haben. Falls Sie in mehreren Ländern gemeldet sind, können Sie mehrere Adressen angeben."@de ,
        "Es geht um das Land, in dem Sie offiziell eine Wohnadresse haben. Wenn Sie in mehreren Ländern gemeldet sind, können Sie mehrere Adressen angeben."@de-x-es ,
        "This refers to the country where your residence is officially registered. If you are registered in multiple countries, you may specify them all."@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:registered_residence_country ;
        sh:property [
            sh:path ff:registered_residence_country ;
            sh:in (ff:registered_residence_country-germany ff:registered_residence_country-other) ;
        ] ;
    ] .

ff:registered_residence_country-germany a ff:AnswerOption ; rdfs:label "Deutschland"@de , 
    "Deutschland"@de-x-es ,
    "Germany"@en .
ff:registered_residence_country-other a ff:AnswerOption ; rdfs:label "Anderes Land"@de , 
    "Anderes Land"@de-x-es ,
    "Other country"@en .


ff:employment_country a ff:DataField ;
    schema:category ff:persoenliche-angaben-df-kategorie;
    rdfs:label "Land des Arbeitsverhältnisses"@de , "Country of employment"@en ;
    schema:question "In welchem Land sind Sie beschäftigt?"@de , 
        "In welchem Land arbeiten Sie?"@de-x-es ,
        "In which country are you employed?"@en ;
    rdfs:comment "Wo sind Sie beschäftigt? Sie können mehrere Länder angeben, wenn Sie in mehreren Ländern arbeiten."@de ,
        "Bitte geben Sie das Land an, in dem Sie arbeiten. Wenn Sie in mehreren Ländern arbeiten, können Sie mehrere Länder nennen."@de-x-es ,
        "Where are you employed? You can specify multiple countries if you work in several countries."@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:employment_country ;
        sh:property [
            sh:path ff:employment_country ;
            sh:in (ff:employment_country-germany ff:employment_country-other) ;
        ] ;
    ] .

ff:employment_country-germany a ff:AnswerOption ; rdfs:label "Deutschland"@de , 
    "Deutschland"@de-x-es ,
    "Germany"@en .
ff:employment_country-other a ff:AnswerOption ; rdfs:label "Anderes Land"@de , 
    "Anderes Land"@de-x-es ,
    "Other country"@en .


ff:disability_expected_duration_6_months a ff:DataField ;
    schema:category ff:gesundheit-pflege-df-kategorie ;
    rdfs:label "Behinderung voraussichtlich länger als 6 Monate"@de , "Disability expected to last longer than 6 months"@en ;
    schema:question "Wird Ihre Behinderung mit hoher Wahrscheinlichkeit länger als 6 Monate andauern?"@de ,
        "Wird Ihre Behinderung voraussichtlich länger als 6 Monate andauern?"@de-x-es ,
        "Is your disability expected to last longer than 6 months?"@en ;
    rdfs:comment "Die Frage soll ermitteln, ob der Zustand Ihrer Behinderung nicht nur vorübergehend besteht, sondern voraussichtlich mindestens sechs Monate andauert."@de ,
        "Die Frage soll klären, ob Ihre Behinderung nicht nur vorübergehend ist, sondern voraussichtlich mindestens sechs Monate andauern wird."@de-x-es ,"Die Frage soll klären, ob Ihre Behinderung nicht nur kurz besteht, sondern voraussichtlich mindestens 6 Monate anhält."@de-x-es ,
        "This question aims to determine whether your disability is not just temporary but is expected to last at least six months."@en ;
        ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:disability_expected_duration_6_months ;
        sh:property [
            sh:path ff:disability_expected_duration_6_months ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;
    ] .


ff:disability_degree_evaluated a ff:DataField ;
    schema:category ff:gesundheit-pflege-df-kategorie ;
    rdfs:label "Grad der Behinderung festgestellt"@de , "Degree of disability evaluated"@en ;
    schema:question "Wurde der Grad Ihrer Behinderung bereits festgestellt?"@de , 
        "Wurde Ihr Grad der Behinderung bereits festgestellt?"@de-x-es ,
        "Has the degree of your disability been evaluated?"@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:disability_degree_evaluated ;
        sh:property [
            sh:path ff:disability_degree_evaluated ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;
    ] .


ff:disability_degree a ff:DataField ;
    schema:category ff:gesundheit-pflege-df-kategorie ;
    rdfs:label "Grad der Behinderung"@de , "Degree of disability"@en ;
    schema:question "Wie hoch ist der Grad Ihrer Behinderung?"@de , 
        "Wie hoch ist Ihr Grad der Behinderung?"@de-x-es ,
        "What is the degree of your disability?"@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:disability_degree ;
        sh:property [
            sh:path ff:disability_degree ;
            sh:datatype xsd:integer ;
            sh:minInclusive 0 ;
            sh:maxInclusive 100 ;
            sh:maxCount 1 ;
        ] ;
    ] .


ff:job_at_risk_without_equal_status a ff:DataField ;
    schema:category ff:arbeitswelt-df-kategorie ;
    rdfs:label "Arbeitsplatz gefährdet ohne Gleichstellung"@de , "Job at risk without equal status"@en ;
    schema:question "Haben Sie wegen Ihrer Behinderung ohne eine Gleichstellung Schwierigkeiten, einen geeigneten Arbeitsplatz zu erlangen oder zu behalten?"@de ,
        "Haben Sie wegen Ihrer Behinderung ohne eine Gleichstellung Schwierigkeiten, einen passenden Arbeitsplatz zu bekommen oder zu behalten?"@de-x-es ,
        "Would you not be able to obtain or retain suitable employment due to your disability without equal status?"@en ;
    rdfs:comment "Gleichstellung bedeutet, dass Personen mit einem Grad der Behinderung von 30 bis 49 formal Schwerbehinderten gleichgestellt werden, wenn sie sonst keinen geeigneten Arbeitsplatz erlangen oder behalten könnten."@de ,
        "Gleichstellung bedeutet: Menschen mit einem Grad der Behinderung von 30 bis 49 werden so behandelt wie schwerbehinderte Menschen. Das gilt, wenn sie sonst keinen passenden Arbeitsplatz bekommen oder behalten könnten."@de-x-es ,
        "Equal status means that persons with a degree of disability of 30 to 49 are formally treated as severely disabled if they would otherwise not be able to obtain or retain suitable employment."@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:job_at_risk_without_equal_status ;
        sh:property [
            sh:path ff:job_at_risk_without_equal_status ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;
    ] .

ff:open_market_fit a ff:DataField ;
    schema:category ff:arbeitswelt-df-kategorie ;
    rdfs:label "Geeignet für den allgemeinen Arbeitsmarkt"@de , "Fit for the open labor market"@en ;
    schema:question "Sind Sie außerhalb einer geschützten Einrichtung in der Lage, auf dem regulären Arbeitsmarkt zu arbeiten?"@de ,
        "Können Sie außerhalb einer geschützten Einrichtung auf dem normalen Arbeitsmarkt arbeiten?"@de-x-es ,
        "Are you able to work on the regular labor market outside a sheltered facility?"@en ;
    rdfs:comment "Der allgemeine Arbeitsmarkt sind alle normalen Arbeitsplätze in Firmen, an denen Menschen – auch mit Hilfsmitteln oder Unterstützung – arbeiten können, solange es kein dauerhaft geschützter Sonder­arbeitsplatz wie eine Werkstatt für behinderte Menschen ist."@de ,
        "Der normale Arbeitsmarkt bedeutet: alle üblichen Arbeitsplätze in Firmen. Dort können auch Menschen mit Hilfsmitteln oder Unterstützung arbeiten. Geschützte Arbeitsplätze wie eine Werkstatt für behinderte Menschen gehören nicht dazu."@de-x-es ,
        "The open labor market includes all regular jobs in companies where people can work, even with aids or support, as long as it is not a permanently protected special workplace like a workshop for disabled people."@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:open_market_fit ;
        sh:property [
            sh:path ff:open_market_fit ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;
    ] .

ff:open_market_hours a ff:DataField ;
    schema:category ff:arbeitswelt-df-kategorie ;
    rdfs:label "Stunden pro Tag auf dem allgemeinen Arbeitsmarkt"@de , "Hours per day on the open labor market"@en ;
    schema:question "Wie viele Stunden könnten Sie unter den üblichen Bedingungen des allgemeinen Arbeitsmarktes pro Tag arbeiten?"@de ,
        "Wie viele Stunden können Sie pro Tag unter normalen Arbeitsbedingungen arbeiten?"@de-x-es ,
        "How many hours could you work per day under the usual conditions of the open labor market?"@en ;
    rdfs:comment "Diese Frage klärt, wie viele Stunden Sie unter den üblichen Bedingungen des allgemeinen Arbeitsmarktes arbeiten können; die Einstufung beruht auf ärztlichen Gutachten und einer sozialmedizinischen Bewertung Ihres verbliebenen Leistungsvermögens."@de ,
        "Die Frage soll zeigen, wie viele Stunden Sie am Tag arbeiten können. Grundlage sind ärztliche Gutachten und eine sozialmedizinische Einschätzung Ihrer Leistungsfähigkeit."@de-x-es ,
        "This question clarifies how many hours you can work per day under the usual conditions of the open labor market; the classification is based on medical reports and a social medical assessment of your remaining capacity."@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:open_market_hours ;
        sh:property [
            sh:path ff:open_market_hours ;
            sh:in (ff:open_market_hours-3hours ff:open_market_hours-6hours ff:open_market_hours-more_than_6hours) ;
            sh:maxCount 1 ;
        ] ;
    ] .

ff:open_market_hours-3hours a ff:AnswerOption ;
    rdfs:label "Höchstens 3 Stunden"@de , "Bis zu 3 Stunden"@de-x-es , "At most 3 hours"@en ;
    rdfs:comment "[dev] Hier eine Erklärung zu dieser Antwortoption"@de , "[dev] Here is an explanation of this answer option"@en .
ff:open_market_hours-6hours a ff:AnswerOption ; rdfs:label "Höchstens 6 Stunden"@de , 
    "Bis zu 6 Stunden"@de-x-es ,
    "At most 6 hours"@en .
ff:open_market_hours-more_than_6hours a ff:AnswerOption ; rdfs:label "Mehr als 6 Stunden"@de , 
    "Mehr als 6 Stunden"@de-x-es ,
    "More than 6 hours"@en .

ff:special_cause_inability a ff:DataField ;
    schema:category ff:arbeitswelt-df-kategorie ;
    rdfs:label "Besondere Gründe für Arbeitsunfähigkeit"@de , "Special reasons for inability to work"@en ;
    schema:question "Ist Ihre Erwerbsminderung durch einen Arbeitsunfall, eine Berufskrankheit, einen Unfall im Wehr- oder Freiwilligendienst, einen Impfschaden oder durch Gewalt entstanden?"@de ,
        "Ist Ihre Erwerbsminderung durch einen Arbeitsunfall, eine Krankheit durch den Beruf, einen Unfall im Wehr- oder Freiwilligendienst, einen Impfschaden oder durch Gewalt entstanden?"@de-x-es ,
        "Is your disability due to a work accident, an occupational disease, an accident during military or voluntary service, a vaccination injury, or violence?"@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:special_cause_inability ;
        sh:property [
            sh:path ff:special_cause_inability ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;
    ] .

ff:general_waiting_period a ff:DataField ;
    schema:category ff:arbeitswelt-df-kategorie ;
    rdfs:label "Allgemeine Wartezeit"@de , "Regular waiting period"@en ;
    schema:question "Haben Sie insgesamt mindestens fünf Jahre Zeiten, in denen Beiträge zur gesetzlichen Rentenversicherung gezahlt wurden oder angerechnete Monate zählen?"@de ,
        "Haben Sie insgesamt mindestens 5 Jahre in der gesetzlichen Rentenversicherung? Dazu zählen gezahlte Beiträge oder angerechnete Zeiten."@de-x-es , 
        "Do you have a total of at least five years of periods during which contributions to the statutory pension insurance were paid or credited months count?"@en ;
    rdfs:comment "Die allgemeine Wartezeit gilt als erfüllt, wenn sich mindestens 60 Kalendermonate mit Beiträgen oder mit gleichgestellten Monaten angesammelt haben; gleichgestellte Monate sind Zeiträume ohne Beiträge, die trotzdem zählen, etwa während Kindererziehung, Schul- oder Studienbesuch, Krankheit, Reha oder Pflege eines Angehörigen."@de ,
        "Die Wartezeit ist erfüllt, wenn Sie mindestens 60 Monate haben. Gezählt werden Monate mit Beiträgen und bestimmte Monate ohne Beiträge. Dazu gehören zum Beispiel Kindererziehung, Schule oder Studium, Krankheit, Reha oder Pflege."@de-x-es ,
        "The regular waiting period is considered fulfilled if at least 60 calendar months with contributions or credited months have accumulated; credited months are periods without contributions that still count, such as during child-rearing, school or university attendance, illness, rehabilitation, or caregiving for a relative."@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:general_waiting_period ;
        sh:property [
            sh:path ff:general_waiting_period ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;
    ] .

ff:extended_waiting_period a ff:DataField ;
    schema:category ff:arbeitswelt-df-kategorie ;
    rdfs:label "Erweiterte Wartezeit"@de , "Extended waiting period"@en ;
    schema:question "Haben Sie, seit Sie ununterbrochen voll erwerbsgemindert sind, insgesamt mindestens 20 Jahre Zeiten in der gesetzlichen Rentenversicherung angesammelt?"@de ,
        "Haben Sie, seit Sie dauerhaft voll erwerbsgemindert sind, mindestens 20 Jahre Zeiten in der gesetzlichen Rentenversicherung gesammelt?"@de-x-es ,
        "Since you have been continuously fully incapable of work, have you accumulated at least 20 years of credited periods in the German statutory pension insurance?"@en ;
    rdfs:comment "Diese besondere Wartezeit gilt für Personen, die schon vor den ersten fünf Beitragsjahren voll erwerbsgemindert waren und es bis heute ohne Unterbrechung geblieben sind. In diesem Fall zählen 240 Kalendermonate rentenrechtlicher Zeiten – meist Anrechnungs- oder Berücksichtigungszeiten wie Schul-, Reha-, Krankheits- oder Pflegemonate –, auch wenn während dieser Zeit kaum oder keine Beiträge gezahlt werden konnten."@de ,
        "Diese besondere Regel gilt für Menschen, die schon früh voll erwerbsgemindert waren und es bis heute ohne Pause geblieben sind. In diesem Fall zählen 240 Monate. Dazu gehören zum Beispiel Schulzeiten, Reha, Krankheit oder Pflege. Auch wenn in dieser Zeit keine oder nur wenige Beiträge gezahlt wurden."@de-x-es ,
        "This special waiting period applies to individuals who were already fully incapable of work before the first five contribution years and have remained so continuously until today. In this case, 240 calendar months of pension-relevant periods count – usually credited or recognized periods such as school, rehabilitation, illness, or caregiving months – even if few or no contributions could be paid during this time."@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:extended_waiting_period ;
        sh:property [
            sh:path ff:extended_waiting_period ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;
    ] .

ff:compulsory_contributions a ff:DataField ;
    schema:category ff:arbeitswelt-df-kategorie ;
    rdfs:label "Pflichtbeiträge zur gesetzlichen Rentenversicherung"@de , "Compulsory contributions to statutory pension insurance"@en ;
    schema:question "Haben Sie in den letzten 5 Jahren mindestens 36 Monate Pflichtbeiträge zur gesetzlichen Rentenversicherung gezahlt?"@de ,
        "Haben Sie in den letzten 5 Jahren mindestens 36 Monate in die gesetzliche Rentenversicherung eingezahlt?"@de-x-es ,
        "Have you paid at least 36 months of compulsory contributions to the statutory pension insurance in the last 5 years?"@en ;
    rdfs:comment "Pflichtbeiträge sind Beträge, die aus einer versicherten Beschäftigung oder einer Pflichtversicherung automatisch abgeführt werden. Der Prüfzeitraum umfasst normalerweise die letzten fünf Jahre vor Eintritt der Erwerbsminderung; er verlängert sich jedoch um beitragsfreie Monate wie Krankheits- oder Reha­zeiten, Zeiten einer bereits gezahlten Erwerbsminderungsrente, Kinder- oder Pflegezeiten, bestimmte unbezahlte Auszeiten sowie bis zu sieben Jahre Schul- oder Studienzeit nach dem 17. Geburtstag. Dadurch können auch weiter zurückliegende Pflichtbeiträge berücksichtigt werden."@de ,
        "Pflichtbeiträge sind feste Beiträge zur Rentenversicherung. Sie werden automatisch von Ihrem Lohn oder Gehalt abgezogen. Normalerweise schaut man auf die letzten 5 Jahre. Zeiten ohne Beiträge zählen nicht mit, zum Beispiel bei Krankheit, Reha, Kindererziehung oder Pflege. Auch Schul- oder Studienzeiten nach dem 17. Geburtstag können angerechnet werden."@de-x-es ,
        "Compulsory contributions are amounts automatically deducted from an insured employment or compulsory insurance. The review period usually covers the last five years before the onset of disability; however, it extends to include contribution-free months such as illness or rehabilitation periods, periods of already paid disability pensions, child-rearing or caregiving periods, certain unpaid breaks, and up to seven years of school or university time after the age of 17. This allows for consideration of compulsory contributions made further back in time."@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:compulsory_contributions ;
        sh:property [
            sh:path ff:compulsory_contributions ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;
    ] .

ff:has_confirmed_addiction a ff:DataField ;
    schema:category ff:gesundheit-pflege-df-kategorie ;
    rdfs:label "Bestätigte Sucht"@de , "Confirmed addiction"@en ;
    schema:question "Leiden Sie an einer Suchterkrankung?"@de , "Do you suffer from an addiction?"@en ;
    rdfs:comment "Eine Sucht wird von einer Ärzt:in bestätigt, wenn mehrere eindeutige Anzeichen vorliegen – zum Beispiel, dass der Konsum nicht mehr kontrolliert werden kann, der Körper ohne die Substanz starke Beschwerden zeigt, ein sehr starkes Verlangen danach besteht und andere wichtige Dinge im Leben vernachlässigt werden."@de ,
        "An addiction is confirmed by a doctor or psychologist when there are multiple clear signs – for example, that consumption can no longer be controlled, the body shows severe discomfort without the substance, there is a very strong craving for it, and other important aspects of life are neglected."@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:has_confirmed_addiction ;
        sh:property [
            sh:path ff:has_confirmed_addiction ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;
    ] .

ff:has_opioid_addiction a ff:DataField ;
    schema:category ff:gesundheit-pflege-df-kategorie ;
    rdfs:label "Opioidabhängigkeit"@de , "Opioid addiction"@en ;
    schema:question "Liegt bei Ihnen eine ärztlich anerkannte Opioid-Abhängigkeitserkrankung vor?"@de , "Sind Sie abhängig von Opioiden und wurde diese Abhängigkeit von einem Arzt oder einer Ärztin bestätigt?"@de-x-es , "Do you have a medically recognized opioid addiction?"@en ;
    rdfs:comment "Eine Substitutionstherapie ist eine Therapieform, die nur bei Personen mit einer Opioidabhängigkeit vermittelt wird."@de , "Eine Substitutionstherapie ist nur für Menschen mit einer Abhängigkeit von Opioiden."@de-x-es , "Substitution therapy is a form of treatment that is only provided to individuals with opioid dependence."@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:has_opioid_addiction ;
        sh:property [
            sh:path ff:has_opioid_addiction ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;
    ] .

ff:has_medical_insurance a ff:DataField ;
    schema:category ff:gesundheit-pflege-df-kategorie ;
    rdfs:label "Krankenversicherung"@de , "Health insurance"@en ;
    schema:question "Sind Sie krankenversichert?"@de , "Sind Sie krankenversichert?"@de-x-es , "Do you have health insurance?"@en ;
    rdfs:comment "Die Kosten einer Substitutionstherapie können nur mit der Krankenkasse abgerechnet werden."@de , "Eine Substitutionstherapie kann nur die Krankenkasse bezahlen."@de-x-es , "The costs of substitution therapy can only be billed to the health insurance company."@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:has_medical_insurance ;
        sh:property [
            sh:path ff:has_medical_insurance ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;
    ] .

ff:sobriety_wish a ff:DataField ;
    schema:category ff:gesundheit-pflege-df-kategorie ;
    rdfs:label "Abstinenz Wunsch"@de , "Sobriety wish"@en ;
    schema:question "Besteht bei Ihnen der Wunsch, abstinent von Opioiden, also vollkommen ohne Opioide zu leben?"@de , "Haben Sie den Wunsch, in Ihrem Leben keine Opioide mehr zu nehmen?"@de-x-es , "Do you wish to abstain from opioids, i.e., to live completely without opioids?"@en ;
    rdfs:comment "Der Wille zur Abstinenz ist in der Regel ein Kriterium, unter dem eine Substitutionstherapie begonnen wird."@de , "Der Wille, keine Opioide zu nehmen, ist meistens eine Bedingung für die Substitutions-Therapie."@de-x-es , "The desire to abstain is usually a criterion for starting substitution therapy."@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:sobriety_wish ;
        sh:property [
            sh:path ff:sobriety_wish ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;
    ] .

ff:has_severe_disease a ff:DataField ;
    schema:category ff:gesundheit-pflege-df-kategorie ;
    rdfs:label "Schwere Erkrankung"@de , "Severe disease"@en ;
    schema:question "Liegt bei Ihnen eine schwere Erkrankung neben der Opiatabhängigkeit vor, wie zum Beispiel Krebs, HIV, Hepatitis A, Hepatitis C?"@de , "Haben Sie eine schwere Krankheit neben der Opiat-Abhängigkeit? Zum Beispiel Krebs, HIV, Hepatitis A oder Hepatitis C?"@de-x-es , "Do you have a serious illness in addition to opioid dependence, such as cancer, HIV, hepatitis A, or hepatitis C?"@en ;
    rdfs:comment "Eine schwere Krankheit neben der Abhängigkeit kann Einfluss darauf haben, ob eine Substitutionstherapie auch ohne Willen zur Abstinenz genehmigt wird."@de , "Eine schwere Krankheit neben der Abhängigkeit kann dafür sorgen, dass man auch ohne den Willen, keine Opioide zu nehmen, eine Substitutionstherapie bekommt."@de-x-es , "A serious illness in addition to addiction can influence whether substitution therapy is approved even without the desire for abstinence."@en ;
    ff:hasShaclShape [
        a sh:NodeShape ;
        sh:targetSubjectsOf ff:has_severe_disease ;
        sh:property [
            sh:path ff:has_severe_disease ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
        ] ;
    ] .

ff:pregnant a ff:DataField ;
